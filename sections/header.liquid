{% style %}
	.section-header {
		position: sticky; /* This is for fixing a Safari z-index issue. PR #2147 */
		margin-bottom: 0;
	}
	.cart-count-bubble {
		--color-cartbubble-header: {{ section.settings.colors_foreground_header_cartbubble }};
		{% if request.page_type == 'index' %}
		--color-cartbubble-header: {{ section.settings.colors_foreground_header_cartbubble_homepage }};
		{% endif %}
	}

	:root:root .search-modal__content {
		--color-base-background-1: {{ section.settings.colors_background_1_search.red }}, {{ section.settings.colors_background_1_search.green }}, {{ section.settings.colors_background_1_search.blue }};
		--gradient-base-background-1: {% if section.settings.gradient_background_1_search != blank %}{{ section.settings.gradient_background_1_search }}{% else %}{{ section.settings.colors_background_1_search }}{% endif %};
		--color-foreground: {{ section.settings.colors_foreground_1_search.red }}, {{ section.settings.colors_foreground_1_search.green }}, {{ section.settings.colors_foreground_1_search.blue }};
	}

	:root:root .predictive-search {
		--color-base-background-1: {{ section.settings.colors_background_1_search_dropdown.red }}, {{ section.settings.colors_background_1_search_dropdown.green }}, {{ section.settings.colors_background_1_search_dropdown.blue }};
		--gradient-base-background-1: {% if section.settings.gradient_background_1_search_dropdown != blank %}{{ section.settings.gradient_background_1_search_dropdown }}{% else %}{{ section.settings.colors_background_1_search_dropdown }}{% endif %};
		--color-foreground: {{ section.settings.colors_foreground_1_search_dropdown.red }}, {{ section.settings.colors_foreground_1_search_dropdown.green }}, {{ section.settings.colors_foreground_1_search_dropdown.blue }};
	}

	:root:root .section-header {
		--header-bottom-border-color: {{ section.settings.header_bottom_border_color.red }}, {{ section.settings.header_bottom_border_color.green }}, {{ section.settings.header_bottom_border_color.blue }};
		--header-bottom-box-shadow: 0 6px 10px -5px #86868633;
	}

	/* sticky header variable settings */
	:root:root .section-header {
		--header-icons-foreground-color: {{ section.settings.header_icons_foreground_color }};
		--header-icons-hover-foreground-color: {{ section.settings.header_icons_hover_foreground_color }};
		--header-logo-foreground-color: {{ section.settings.header_logo_foreground_color }};
		--header-logo-hover-foreground-color: {{ section.settings.header_logo_hover_foreground_color }};
		--color-base-background-1: {{ section.settings.colors_background_1_header.red }}, {{ section.settings.colors_background_1_header.green }}, {{ section.settings.colors_background_1_header.blue }};
		--gradient-base-background-1: {% if section.settings.gradient_background_1_header != blank %}{{ section.settings.gradient_background_1_header }}{% else %}{{ section.settings.colors_background_1_header }}{% endif %};
		--header-nav-parent-foreground-color: {{ section.settings.header_nav_parent_foreground_color }};
		--header-nav-parent-hover-foreground-color: {{ section.settings.header_nav_parent_hover_foreground_color }};
		--header-nav-childless-parent-hover-foreground-color: {{ section.settings.header_nav_childless_parent_hover_foreground_color }};
		--header-nav-parent-hover-underline: {{ section.settings.header_nav_parent_hover_underline }};
