			--header-icons-foreground-color: {{ section.settings.header_icons_foreground_color_hompepage }};
			--header-icons-hover-foreground-color: {{ section.settings.header_icons_hover_foreground_color_homepage }};
			--header-logo-foreground-color: {{ section.settings.header_logo_foreground_color_hompepage }};
			--header-logo-hover-foreground-color: {{ section.settings.header_logo_hover_foreground_color_homepage }};
			--color-base-background-1: {{ section.settings.colors_background_1_header_homepage.red }}, {{ section.settings.colors_background_1_header_homepage.green }}, {{ section.settings.colors_background_1_header_homepage.blue }};
			--gradient-base-background-1: {% if section.settings.gradient_background_1_header_homepage != blank %}{{ section.settings.gradient_background_1_header_homepage }}{% else %}{{ section.settings.colors_background_1_header_homepage }}{% endif %};
			--header-nav-parent-foreground-color: {{ section.settings.header_nav_parent_foreground_color_homepage }};
			--header-nav-parent-hover-foreground-color: {{ section.settings.header_nav_parent_hover_foreground_color_homepage }};
