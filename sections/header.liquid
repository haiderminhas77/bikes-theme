			font-size: {{ settings.heading_size_desktop | divided_by: 10.0 }}rem;
		}

		.cart-modal__empty-message-container .cart__subheading p {
			font-size: {{ settings.subheading_size_desktop | divided_by: 10.0 }}rem;
		}
    }

