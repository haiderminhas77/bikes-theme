{% comment %}
    Renders 'ds-lifestyle-product-tile' in sections everywhere
	previously 'image-with-text'
{% endcomment %}

{{ 'component-image-with-text.css' | asset_url | stylesheet_tag }}
{{ 'component-card.css' | asset_url | stylesheet_tag }}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-badges.css' | asset_url | stylesheet_tag }}

{% comment %}
	Renders 'ds-video' in sections everywhere
{% endcomment %}

{{ 'video-section.css' | asset_url | stylesheet_tag }}
{{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

{% comment %}
    Re-occuring capture statements for arguments not directly passable to image_tag
{% endcomment %}

{%- capture cover_image_height -%}{{ section.settings.cover_image.width | divided_by: section.settings.cover_image.aspect_ratio | round }}{%- endcapture -%}
{%- capture cover_image_alt -%}{{ 'sections.video.load_video' | t: description: section.settings.description | escape }}{%- endcapture -%}
{%- capture section_image_height -%}{{ section.settings.image.width | divided_by: section.settings.image.aspect_ratio | round }}{%- endcapture -%}

{%- style -%}

{% if section.settings.background_color_full_width != true %}
    #shopify-section-{{ section.id }} {
        background: unset;
    }
{% endif %}
	.section-{{ section.id }}-padding {
		margin-top: {{ section.settings.margin_top_mobile }}px;
		margin-bottom: {{ section.settings.margin_bottom_mobile }}px;
        padding-top: {{ section.settings.padding_top_mobile }}px;
		padding-bottom: {{ section.settings.padding_bottom_mobile }}px;
        padding-left: {{ section.settings.padding_left_mobile }}px;
		padding-right: {{ section.settings.padding_right_mobile }}px;
	}

	@media screen and (min-width: 750px) {
		.section-{{ section.id }}-padding {
			margin-top: {{ section.settings.margin_top }}px;
			margin-bottom: {{ section.settings.margin_bottom }}px;
            padding-top: {{ section.settings.padding_top }}px;
			padding-bottom: {{ section.settings.padding_bottom }}px;
            padding-left: {{ section.settings.padding_left }}px;
			padding-right: {{ section.settings.padding_right }}px;
		}
	}

      /* grid variables for promo grid gaps  */
    :root:root #shopify-section-{{ section.id }} .image-with-text__grid {
        --grid-mobile-horizontal-spacing: {{ section.settings.spacing_grid_horizontal_mob | divided_by: 10.0 }}rem;
        --grid-desktop-horizontal-spacing: {{ section.settings.spacing_grid_horizontal | divided_by: 10.0 }}rem;
        --grid-mobile-vertical-spacing: {{ section.settings.spacing_grid_vertical_mob | divided_by: 10.0 }}rem;
        --grid-desktop-vertical-spacing: {{ section.settings.spacing_grid_vertical | divided_by: 10.0 }}rem;
    }

	:root:root  #shopify-section-{{ section.id }} .image-with-text__grid .image-with-text__text-item.color-background-custom {
		--color-custom-background: {{ section.settings.colors_background_1.red }}, {{ section.settings.colors_background_1.green }}, {{ section.settings.colors_background_1.blue }};
		--gradient-custom-background: {% if section.settings.gradient_background_1 != blank %}{{ section.settings.gradient_background_1 }}{% else %}{{ section.settings.colors_background_1 }}{% endif %};
	}

    :root:root  #shopify-section-{{ section.id }}.color-background-custom {
		--color-custom-background: {{ section.settings.colors_background_1_section.red }}, {{ section.settings.colors_background_1_section.green }}, {{ section.settings.colors_background_1_section.blue }};
		--gradient-custom-background: {% if section.settings.gradient_background_1_section != blank %}{{ section.settings.gradient_background_1_section }}{% else %}{{ section.settings.colors_background_1_section }}{% endif %};
	}

    #shopify-section-{{ section.id }} .card-wrapper .media {
        width: 100%;
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .card-wrapper .media {
            width: 100%;
        }
    }

    #shopify-section-{{ section.id }} .banner__heading p, #shopify-section-{{ section.id }} .banner__caption p, #shopify-section-{{ section.id }} .banner__text p {
        margin: 0;
        padding: 0;
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .image-with-text__content {
            padding-top: {{ section.settings.banner__box_padding_top | divided_by: 10.0 }}rem;
            padding-bottom: {{ section.settings.banner__box_padding_bottom | divided_by: 10.0 }}rem;
            padding-left: {{ section.settings.banner__box_padding_left | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.banner__box_padding_right | divided_by: 10.0 }}rem;
        }

        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .image-with-text__content {
            padding-top: {{ section.settings.banner__box_padding_top_mob | divided_by: 10.0 }}rem;
            padding-bottom: {{ section.settings.banner__box_padding_bottom_mob | divided_by: 10.0 }}rem;
            padding-left: {{ section.settings.banner__box_padding_left_mob | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.banner__box_padding_right_mob | divided_by: 10.0 }}rem;
        }

        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
        }
    }
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .image-with-text__content {
            max-width: {{ section.settings.width_mob }}%;
            margin: auto;
        }
    }
    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .image-with-text__content {
            max-width: {{ section.settings.width }}%;
            margin: auto;
        }
    }

    #shopify-section-{{ section.id }} .image-with-text__text-item {
        position: relative;
    }
    #shopify-section-{{ section.id }} .image-with-text .grid {
        margin-bottom: 0;
        margin-top: 0;
    }

    #shopify-section-{{ section.id }} .product-grid>li.grid__item {
        background-color: unset!important;
    }
    #shopify-section-{{ section.id }} .grid.product-grid {
        width: 100%;
    }

    #shopify-section-{{ section.id }} .video-section__media {
        height: 100%;
    }
    #shopify-section-{{ section.id }}  .image-with-text__media-item {
        height: 100%;
    }
{%- endstyle -%}


{%- if section.settings.image_mob != blank -%}
    {%- style -%}
        @media screen and (max-width: 749px) {
            #shopify-section-{{ section.id }} .image-with-text__media--adapt {
                {% if section.settings.image_ratio == 'adapt' %}
                    padding-bottom: {{ 1 | divided_by: section.settings.image_mob.aspect_ratio | times: 100 }}%;
                {% elsif section.settings.image_ratio == 'square' %}
                    padding-bottom: 100%;
                {% elsif section.settings.image_ratio == 'portrait' %}
                    padding-bottom: 125%;
                {% elsif section.settings.image_ratio == '16-9' %}
                    padding-bottom: calc(9/16 * 100%);
                {% elsif section.settings.image_ratio == '4-3' %}
                    padding-bottom: calc(3/4 * 100%);
                {% elsif section.settings.image_ratio == '3-4' %}
                    padding-bottom: calc(4/3 * 100%);
                {% elsif section.settings.image_ratio == '2-1' %}
                    padding-bottom:  calc(2/1 * 100%);
                {% endif %}
                    content: '';
                    display: block;
            }
        }

        @media screen and (min-width: 750px) {
            #shopify-section-{{ section.id }} .image-with-text__media--adapt {
                {% if section.settings.image_ratio == 'adapt' %}
                    padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;
                {% elsif section.settings.image_ratio == 'square' %}
                    padding-bottom: 100%;
                {% elsif section.settings.image_ratio == 'portrait' %}
                    padding-bottom: 125%;
                {% elsif section.settings.image_ratio == '16-9' %}
                    padding-bottom: calc(9/16 * 100%);
                {% elsif section.settings.image_ratio == '4-3' %}
                    padding-bottom: calc(3/4 * 100%);
                {% elsif section.settings.image_ratio == '3-4' %}
                    padding-bottom: calc(4/3 * 100%);
                {% elsif section.settings.image_ratio == '2-1' %}
                    padding-bottom:  calc(2/1 * 100%);
                {% endif %}
                    content: '';
                    display: block;
                }
            }
    {%- endstyle -%}
{%- elsif section.settings.image != blank -%}
    {%- style -%}
    #shopify-section-{{ section.id }} .image-with-text__media--adapt {
        {% if section.settings.image_ratio == 'adapt' %}
            padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;
        {% elsif section.settings.image_ratio == 'square' %}
            padding-bottom: 100%;
        {% elsif section.settings.image_ratio == 'portrait' %}
            padding-bottom: 125%;
        {% elsif section.settings.image_ratio == '16-9' %}
            padding-bottom: calc(9/16 * 100%);
        {% elsif section.settings.image_ratio == '4-3' %}
            padding-bottom: calc(3/4 * 100%);
        {% elsif section.settings.image_ratio == '3-4' %}
            padding-bottom: calc(4/3 * 100%);
        {% elsif section.settings.image_ratio == '2-1' %}
            padding-bottom:  calc(2/1 * 100%);
        {% endif %}
            content: '';
            display: block;
        }
    {%- endstyle -%}
{%- endif -%}

{% comment %} video styles {% endcomment %}
{%- if section.settings.cover_image != blank -%}
	{%- style -%}
	.video-slide--background {
		-o-object-fit: cover;
		object-fit: cover;
		-o-object-position: center center;
		object-position: center center;
		width: 100%;
		display: block;
		height: 100%;
		position: absolute;
		top: 0;
		left: 0;
	}
	{%- endstyle -%}
{%- endif -%}

{%- if section.settings.cover_image_mob != blank -%}
	{%- style -%}
		@media screen and (max-width: 749px) {
			#shopify-section-{{ section.id }} .video-section__media {
				padding-bottom: {{ 1 | divided_by: section.settings.cover_image_mob.aspect_ratio | times: 100 }}%;
			}
		}

		@media screen and (min-width: 750px) {
			#shopify-section-{{ section.id }} .video-section__media {
				padding-bottom: {{ 1 | divided_by: section.settings.cover_image.aspect_ratio | times: 100 }}%;
			}
		}
	{%- endstyle -%}
{%- elsif section.settings.cover_image != blank -%}
    {%- style -%}
	@media screen and (max-width: 749px) {
		#shopify-section-{{ section.id }} .video-section__media {
			padding-bottom: {{ 1 | divided_by: section.settings.cover_image.aspect_ratio | times: 100 }}%;
		}
	}

	@media screen and (min-width: 750px) {
		#shopify-section-{{ section.id }} .video-section__media {
			padding-bottom: {{ 1 | divided_by: section.settings.cover_image.aspect_ratio | times: 100 }}%;
		}
	}
    {%- endstyle -%}
{%- endif -%}

<div class="section-{{ section.id }}-margins {% if section.settings.page_width == 'normal' %} page-width-desktop-max-width-only{% endif %}">
    <div class="image-with-text isolate section-{{ section.id }}-padding color-background-custom gradient">
        <div class="image-with-text__grid grid grid--gapless grid--1-col grid--{% if section.settings.desktop_image_width == 'medium' %}2-col-tablet{% else %}3-col-tablet{% endif %} {% if section.settings.layout == 'text_first' %} image-with-text__grid--reverse{% endif %}{% if section.settings.layout_mob == 'text_first' %} image-with-text__grid--reverse-mob{% endif %}">
            <div class="image-with-text__media-item image-with-text__media-item--{{ section.settings.desktop_image_width }} image-with-text__media-item--{{ section.settings.desktop_content_position }} grid__item">
                {% comment %} image {% endcomment %}
                {% if section.settings.video_url or section.settings.video_picker %}
                    {% comment %} video {% endcomment %}
                    <div class="video-section">
                        <noscript>
                            <div class="video-section__media"
                                {% if section.settings.cover_image != blank %} style="padding-bottom: {{ 1 | divided_by: section.settings.cover_image.aspect_ratio | times: 100 }}%;"{% endif %}
                            >
                                <a href="{{ section.settings.video_url }}" class="video-section__poster media media--transparent media--landscape{% if section.settings.cover_image == blank %} video-section__placeholder{% endif %}">
                                    {%- if section.settings.cover_image != blank -%}
                                        {% liquid 
                                            assign widths = '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840'
                                        %}
                                        {%- capture sizes -%}"(min-width: 990px) 710px, (min-width: 750px)550px, 550px"{%- endcapture -%}
                                        {% if section.settings.cover_image_mob != blank %}
                                            <picture>
                                                <source srcset="{%- if section.settings.cover_image.width >= 375 -%}{{ section.settings.cover_image | image_url: width: 375 }} 375w,{%- endif -%}
                                                {%- if block.settings.cover_image.width >= 550 -%}{{ block.settings.cover_image | image_url: width: 550 }} 550w,{%- endif -%}
                                                    {%- if section.settings.cover_image.width >= 750 -%}{{ section.settings.cover_image | image_url: width: 750 }} 750w,{%- endif -%}
                                                    {%- if section.settings.cover_image.width >= 1100 -%}{{ section.settings.cover_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                    {%- if section.settings.cover_image.width >= 1500 -%}{{ section.settings.cover_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                    {%- if section.settings.cover_image.width >= 1780 -%}{{ section.settings.cover_image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                    {%- if section.settings.cover_image.width >= 2000 -%}{{ section.settings.cover_image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                    {%- if section.settings.cover_image.width >= 3000 -%}{{ section.settings.cover_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                                                    {%- if section.settings.cover_image.width >= 3840 -%}{{ section.settings.cover_image | image_url: width: 3840 }} 3840w,{%- endif -%}
                                                    {{ section.settings.cover_image | image_url }} {{ section.settings.cover_image.width }}w"
                                                    media="(min-width: 750px)"
                                                    sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                                                        (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %},
                                                        calc(100vw - 30px)"
                                                />
                                                <source srcset="{%- if section.settings.cover_image_mob.width >= 375 -%}{{ section.settings.cover_image_mob | image_url: width: 375 }} 375w,{%- endif -%}
                                                    {%- if block.settings.cover_image_mob.width >= 550 -%}{{ block.settings.cover_image_mob | image_url: width: 550 }} 550w,{%- endif -%}
                                                    {%- if section.settings.cover_image_mob.width >= 750 -%}{{ section.settings.cover_image_mob | image_url: width: 750 }} 750w,{%- endif -%}
                                                    {%- if section.settings.cover_image_mob.width >= 1100 -%}{{ section.settings.cover_image_mob | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                    {%- if section.settings.cover_image_mob.width >= 1500 -%}{{ section.settings.cover_image_mob | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                    {%- if section.settings.cover_image_mob.width >= 1780 -%}{{ section.settings.cover_image_mob | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                    {%- if section.settings.cover_image_mob.width >= 2000 -%}{{ section.settings.cover_image_mob | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                    {%- if section.settings.cover_image_mob.width >= 3000 -%}{{ section.settings.cover_image_mob | image_url: width: 3000 }} 3000w,{%- endif -%}
                                                    {%- if section.settings.cover_image_mob.width >= 3840 -%}{{ section.settings.cover_image_mob | image_url: width: 3840 }} 3840w,{%- endif -%}
                                                    {{ section.settings.cover_image_mob | image_url }} {{ section.settings.cover_image_mob.width }}w"
                                                    media="(max-width: 749px)"
                                                    sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                                                        (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %},
                                                        calc(100vw - 30px)"
                                                />
                                                {{ section.settings.cover_image | image_url: width: 700 | image_tag:
                                                    loading: 'lazy',
                                                    height: section.settings.cover_image.height,
                                                    width: section.settings.cover_image.width,
                                                    alt: cover_image_alt,
                                                    widths: widths,
                                                    sizes: "(min-width: 990px) 710px, (min-width: 750px) 550px, 550px"}}
                                            </picture>
                                        {% else %}
                                            {{ section.settings.cover_image | image_url: width: 700 | image_tag:
                                            loading: 'lazy',
                                            sizes: sizes,
                                            height: section.settings.cover_image.width,
                                            width: cover_image_height,
                                            widths: widths }}
                                        {% endif %}
                                    {%- else -%}
                                        {{ 'collection-2' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                                    {%- endif -%}
                                </a>
                            </div>
                        </noscript>
                        <deferred-media class="video-section__media deferred-media no-js-hidden{% if template.name == 'product' %} product__lifestyle-video{% endif %}" data-media-id="{{ section.settings.video_url.id }}">
                            <button
                                id="Deferred-Poster-Modal-{{ section.settings.video_url.id }}"
                                class="video-section__poster media deferred-media__poster media--landscape"
                                type="button"
                            >
                                {%- if section.settings.cover_image != blank -%}
                                    {% liquid 
                                        assign widths = '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840'
                                    %}
                                    {%- capture sizes -%}"(min-width: 990px) 710px, (min-width: 750px)550px, 550px"{%- endcapture -%}
                                    {% if section.settings.cover_image_mob != blank %}
                                        <picture>
                                            <source srcset="{%- if section.settings.cover_image.width >= 375 -%}{{ section.settings.cover_image | image_url: width: 375 }} 375w,{%- endif -%}
                                            {%- if block.settings.cover_image.width >= 550 -%}{{ block.settings.cover_image | image_url: width: 550 }} 550w,{%- endif -%}
                                                {%- if section.settings.cover_image.width >= 750 -%}{{ section.settings.cover_image | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if section.settings.cover_image.width >= 1100 -%}{{ section.settings.cover_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if section.settings.cover_image.width >= 1500 -%}{{ section.settings.cover_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if section.settings.cover_image.width >= 1780 -%}{{ section.settings.cover_image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if section.settings.cover_image.width >= 2000 -%}{{ section.settings.cover_image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if section.settings.cover_image.width >= 3000 -%}{{ section.settings.cover_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                                                {%- if section.settings.cover_image.width >= 3840 -%}{{ section.settings.cover_image | image_url: width: 3840 }} 3840w,{%- endif -%}
                                                {{ section.settings.cover_image | image_url }} {{ section.settings.cover_image.width }}w"
                                                media="(min-width: 750px)"
                                                sizes="{% if section.settings.page_width == 'full' %}100vw{% else %}(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 }}px, (min-width: 750px) calc(100vw - 10rem), 100vw{% endif %}"
                                            />
                                            <source srcset="{%- if section.settings.cover_image_mob.width >= 375 -%}{{ section.settings.cover_image_mob | image_url: width: 375 }} 375w,{%- endif -%}
                                            {%- if block.settings.cover_image_mob.width >= 550 -%}{{ block.settings.cover_image_mob | image_url: width: 550 }} 550w,{%- endif -%}
                                                {%- if section.settings.cover_image_mob.width >= 750 -%}{{ section.settings.cover_image_mob | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if section.settings.cover_image_mob.width >= 1100 -%}{{ section.settings.cover_image_mob | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if section.settings.cover_image_mob.width >= 1500 -%}{{ section.settings.cover_image_mob | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if section.settings.cover_image_mob.width >= 1780 -%}{{ section.settings.cover_image_mob | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if section.settings.cover_image_mob.width >= 2000 -%}{{ section.settings.cover_image_mob | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if section.settings.cover_image_mob.width >= 3000 -%}{{ section.settings.cover_image_mob | image_url: width: 3000 }} 3000w,{%- endif -%}
                                                {%- if section.settings.cover_image_mob.width >= 3840 -%}{{ section.settings.cover_image_mob | image_url: width: 3840 }} 3840w,{%- endif -%}
                                                {{ section.settings.cover_image_mob | image_url }} {{ section.settings.cover_image_mob.width }}w"
                                                media="(max-width: 749px)"
                                                sizes="{% if section.settings.page_width == 'full' %}100vw{% else %}(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 }}px, (min-width: 750px) calc(100vw - 10rem), 100vw{% endif %}"
                                            />
                                            {{ section.settings.cover_image | image_url: width: 1920 | image_tag:
                                                loading: 'lazy',
                                                height: section.settings.cover_image.height,
                                                width: section.settings.cover_image.width,
                                                alt: cover_image_alt }}
                                        </picture>
                                    {% else %}
                                        {{ section.settings.cover_image | image_url: width: 1920 | image_tag:
                                        loading: 'lazy',
                                        height: cover_image_height,
                                        width: section.settings.cover_image.width,
                                        sizes: sizes,
                                        widths: widths,
                                        alt: cover_image_alt }}
                                    {% endif %}
                                {%- else -%}
                                    {{ 'collection-2' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                                {%- endif -%}
                                <span class="deferred-media__poster-button motion-reduce">
                                    {%- render 'icon-play' -%}
                                </span>
                            </button>
                            <template>
                                {% if section.settings.video_picker != blank %}
                                    <video id="mp4-video-player" class="video-slide--background" controls playsinline src="{{ section.settings.video_picker.sources[1].url }}" data-desktop-src="{{ section.settings.video_picker.sources[1].url }}" data-mobile-src="{{ section.settings.video_picker.sources[1].url }}"></video>
                                {%- elsif section.settings.video_picker == blank and section.settings.video_url != blank -%}
                                    {%- if section.settings.video_url.type == 'youtube' -%}
                                            <iframe src="https://www.youtube.com/embed/{{ section.settings.video_url.id }}?enablejsapi=1" class="js-youtube" allow="autoplay; encrypted-media" allowfullscreen title="{{ section.settings.description | escape }}"></iframe>
                                        {%- else -%}
                                            <iframe src="https://player.vimeo.com/video/{{ section.settings.video_url.id }}" class="js-vimeo" allow="autoplay; encrypted-media" allowfullscreen title="{{ section.settings.description | escape }}"></iframe>
                                        {%- endif -%}
                                    {% endif %}
                            </template>
                        </deferred-media>
                    </div>
                {% comment %} endvideo {% endcomment %}
                {% else %}
                {% comment %} Image {% endcomment %}
                    <div class="image-with-text__media{% if section.settings.image_ratio != 'custom' %} image-with-text__media--adapt{% else %} image-with-text__media--{{ section.settings.image_height }}{% endif %} {% if section.settings.image != blank %}media{% else %}image-with-text__media--placeholder placeholder{% endif %}">
                        {%- if section.settings.image != blank -%}
                            {% liquid 
                                if section.settings.image_ratio == '2-1'
                                assign widths = '750, 1200'
                                elsif section.settings.image_ratio == 'square' or 'portrait'
                                    assign widths = '750, 950'
                                elsif section.settings.image_ratio == '4-3' or  '3-4'
