								}
                                
                                /* desk primary 2 */
								#shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).button--primary {
									--color-button: {{ block.settings.button_color_2.red }}, {{ block.settings.button_color_2.green }}, {{ block.settings.button_color_2.blue }};
									--color-button-text: {{ block.settings.button_text_color_2.red }}, {{ block.settings.button_text_color_2.green }}, {{ block.settings.button_text_color_2.blue }};
								}

                                /* desk primary 2 [hover] */
                                #shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).button--primary:not([disabled]):hover {
									--color-button: {{ block.settings.button_color_2_hover.red }}, {{ block.settings.button_color_2_hover.green }}, {{ block.settings.button_color_2_hover.blue }};
									--color-button-text: {{ block.settings.button_text_color_2_hover.red }}, {{ block.settings.button_text_color_2_hover.green }}, {{ block.settings.button_text_color_2_hover.blue }};
								}

                                /* desk arrow 2 */
                                #shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).link-with-icon {
									--color-link: {{ block.settings.button_arrow_color_2.red }}, {{ block.settings.button_arrow_color_2.green }}, {{ block.settings.button_arrow_color_2.blue }};
								}

                                /* desk arrow 2 [hover] */
                                #shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).link-with-icon:not([disabled]):hover {
                                    --color-link: {{ block.settings.button_arrow_color_hover_2.red }}, {{ block.settings.button_arrow_color_hover_2.green }}, {{ block.settings.button_arrow_color_hover_2.blue }};
                                }

                                /* desk underline 2 */
                                #shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).button--underline {
									--color-button-text: {{ block.settings.button_arrow_color_2.red }}, {{ block.settings.button_arrow_color_2.green }}, {{ block.settings.button_arrow_color_2.blue }};
								}

                                /* desk underline 2 [hover] */
                                #shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).button--underline:not([disabled]):hover {
                                    --color-button-text: {{ block.settings.button_arrow_color_hover_2.red }}, {{ block.settings.button_arrow_color_hover_2.green }}, {{ block.settings.button_arrow_color_hover_2.blue }};
                                }

                                /* desk outline 2 */
								#shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).button--secondary {
									--color-button: {{ block.settings.button_outline_color_2.red }}, {{ block.settings.button_outline_color_2.green }}, {{ block.settings.button_outline_color_2.blue }};
									--color-button-text: {{ block.settings.button_outline_text_color_2.red }}, {{ block.settings.button_outline_text_color_2.green }}, {{ block.settings.button_outline_text_color_2.blue }};
								}

                                /* desk outline 2 [hover] */
                                #shopify-section-{{ section.id }} .banner .banner__buttons a:nth-of-type(2).button--secondary:not([disabled]):hover {
									--color-button: {{ block.settings.button_outline_color_2_hover.red }}, {{ block.settings.button_outline_color_2_hover.green }}, {{ block.settings.button_outline_color_2_hover.blue }};
									--color-button-text: {{ block.settings.button_outline_text_color_2_hover.red }}, {{ block.settings.button_outline_text_color_2_hover.green }}, {{ block.settings.button_outline_text_color_2_hover.blue }};
                                    --alpha-button-background: 1;
								}
							}

                            /* Desktop custom font size */
                            @media only screen and (min-width: 750px) {
                                #shopify-section-{{ section.id }} .banner .banner__buttons .custom-desktop {
                                    font-size: {{ block.settings.button_size_custom | divided_by: 10.0 }}rem;
                                }
                                #shopify-section-{{ section.id }} .banner__buttons {
                                    gap: {{ block.settings.buttons_gap }}rem;
                                }
                            }
                            /* Mobile custom font size */
                            @media screen and (max-width: 750px) {
                                #shopify-section-{{ section.id }} .banner .banner__buttons .custom-mobile {
                                    font-size: {{ block.settings.button_size_mobile_custom | divided_by: 10.0 }}rem;
                                }
                                #shopify-section-{{ section.id }} .banner__buttons {
                                    gap: {{ block.settings.buttons_gap_mob }}rem;
                                }
                            }

                            #shopify-section-{{ section.id }} .block-{{ block.id }}-padding {
                                margin-top: {{ block.settings.padding_top_mobile | divided_by: 10.0 }}rem;
                                margin-bottom: {{ block.settings.padding_bottom_mobile | divided_by: 10.0 }}rem;
                            }

                            @media screen and (min-width: 750px) {
                                #shopify-section-{{ section.id }} .block-{{ block.id }}-padding {
                                    margin-top: {{ block.settings.padding_top | divided_by: 10.0 }}rem;
                                    margin-bottom: {{ block.settings.padding_bottom | divided_by: 10.0 }}rem;
                                }
                            }
						{% endstyle %}
						<div class="banner__buttons {% if block.settings.button_label_1 != blank and block.settings.button_label_2 != blank %} banner__buttons--multiple{% endif %} block-{{ block.id }}-padding{% if block.settings.buttons_mobile_position == 'stacked' %} banner__buttons--stacked-mobile{% elsif block.settings.buttons_mobile_position == 'full_width' %} banner__buttons--full-width{% endif %}" {{ block.shopify_attributes }}>
							{%- if block.settings.button_label_1 != blank -%}
								<a{% if block.settings.button_link_1 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link_1 }}"{% endif %} {% if block.settings.button_new_tab_1 %}target="_blank"{% endif %} class="{% if block.settings.button_style_1 != 'arrow' %}button{% endif %}{% if block.settings.button_style_1 == 'arrow' %} link link-with-icon{% endif %}{% if block.settings.button_style_1 == 'outline' %} button--secondary{% elsif block.settings.button_style_1 == 'primary' %} button--primary{% elsif block.settings.button_style_1 == 'underline' %} button--underline{% endif %}{% if block.settings.button_uppercase_1 %} uppercase{% endif %} {{ block.settings.button_size }}-desktop {{ block.settings.button_size_mobile }}-mobile{% if block.settings.button_wide_1 %} button--wide{% endif %}">
                                    {{ block.settings.button_label_1 | escape }}{% if block.settings.button_style_1 == 'arrow'%}{%- render 'icon-arrow-right' -%}{% endif %}
                                </a>
							{%- endif -%}
							{%- if block.settings.button_label_2 != blank -%}
								<a{% if block.settings.button_link_2 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link_2 }}"{% endif %} {% if block.settings.button_new_tab_2 %}target="_blank"{% endif %} class="{% if block.settings.button_style_2 != 'arrow' %}button{% endif %}{% if block.settings.button_style_2 == 'arrow' %} link link-with-icon{% endif %}{% if block.settings.button_style_2 == 'outline' %} button--secondary{% elsif block.settings.button_style_2 == 'primary' %} button--primary{% elsif block.settings.button_style_2 == 'underline' %} button--underline{% endif %}{% if block.settings.button_uppercase_2 %} uppercase{% endif %} {{ block.settings.button_size }}-desktop {{ block.settings.button_size_mobile }}-mobile{% if block.settings.button_wide_2 %} button--wide{% endif %}">
                                    {{ block.settings.button_label_2 | escape }}{% if block.settings.button_style_2 == 'arrow'%}{%- render 'icon-arrow-right' -%}{% endif %}</a>
							{%- endif -%}
						</div>
					{%- when 'caption' -%}
						{% style %}
							#shopify-section-{{ section.id }} .banner__caption {
								color: rgba(var(--color-base-text), 1);
							}
                            #shopify-section-{{ section.id }} .banner__caption a {
								color: rgba(var(--color-base-text), 1);
							}

                            #shopify-section-{{ section.id }} .banner__caption p {
                                margin: unset;
                                padding: unset;
                            }

							@media screen and (max-width: 749px) {
								:root:root  #shopify-section-{{ section.id }} .banner.banner--mobile-bottom .banner__caption {
									--color-base-text: {{ block.settings.caption_below_color.red }}, {{ block.settings.caption_below_color.green }}, {{ block.settings.caption_below_color.blue }};
								}
								:root:root  #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__caption {
									--color-base-text: {{ block.settings.caption_color.red }}, {{ block.settings.caption_color.green }}, {{ block.settings.caption_color.blue }};
								}
                                :root:root  #shopify-section-{{ section.id }} .banner.banner--mobile-bottom .banner__caption a {
									--color-base-text: {{ block.settings.caption_link_below_color }};
								}
								:root:root  #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__caption a {
									--color-base-text:  {{ block.settings.caption_link_color }};
								}
							}

							@media screen and (min-width: 750px) {
								:root:root  #shopify-section-{{ section.id }} .banner__caption {
									--color-base-text: {{ block.settings.caption_color.red }}, {{ block.settings.caption_color.green }}, {{ block.settings.caption_color.blue }};
								}

                                :root:root  #shopify-section-{{ section.id }} .banner__caption a {
									--color-base-text: {{ block.settings.text_link_color }};
								}
							}

                            .block-{{ block.id }}-padding {
                                margin-top: {{ block.settings.padding_top_mobile | divided_by: 10.0 }}rem;
                                margin-bottom: {{ block.settings.padding_bottom_mobile | divided_by: 10.0 }}rem;
                            }

                            @media screen and (min-width: 750px) {
                                .block-{{ block.id }}-padding {
                                    margin-top: {{ block.settings.padding_top | divided_by: 10.0 }}rem;
                                    margin-bottom: {{ block.settings.padding_bottom | divided_by: 10.0 }}rem;
                                }
                            }

                            /* Desktop custom font size */
                            @media screen and (min-width: 1024px) {
                                #shopify-section-{{ section.id }} .banner__caption.custom-desktop {
                                    font-size: {{ block.settings.caption_size_custom | divided_by: 10.0 }}rem;
                                }
                            }
                            /* Mobile custom font size */
                            @media screen and (max-width: 1023px) {
                                #shopify-section-{{ section.id }} .banner__caption.custom-mobile {
                                    font-size: {{ block.settings.caption_size_mobile_custom | divided_by: 10.0 }}rem;
                                }
                            }
                            #shopify-section-{{ section.id }} .banner__caption {
                                font-family: var(--font-caption-family);
                                line-height: 0.9;
                                font-style: var(--font-caption-style);
                                {% if block.settings.caption_font_weight == 'bold' %}
                                    font-weight: var(--font-caption-weight-bold);
                                {% elsif block.settings.caption_font_weight == 'light' %}
                                    font-weight: var(--font-caption-weight-light);
                                {% else %}
                                    font-weight: var(--font-caption-weight);
                                {% endif %}
                                {% if block.settings.caption_letter_spacing == 'wide' %}
                                    letter-spacing: 0.09rem;
                                {% endif %}
                            }

						{% endstyle %}
						<div class="banner__caption {{ block.settings.caption_size }}-desktop {{ block.settings.caption_size_mobile }}-mobile{% if block.settings.caption_uppercase %} uppercase{% endif %} block-{{ block.id }}-padding" {{ block.shopify_attributes }}>{{ block.settings.caption }}</div>
                    {%- when 'image' -%}
                        <div class="banner__image block-{{ block.id }}-padding" {{ block.shopify_attributes }}>
                            <div class="media media--transparent" style="padding-top: {{ 1 | divided_by: block.settings.header_image.aspect_ratio | times: 100 }}%">
                                {%- if block.settings.header_image != blank -%}	
                                    {% assign header_image_width = block.settings.header_image_width %}
    
                                    {% assign header_image_width_string = block.settings.header_image_width | append: ''%}
                                    {{ block.settings.header_image | image_url: width: header_image_width | image_tag:	
                                    loading: 'lazy',	
                                    height: block.settings.header_image.height,
                                    width: block.settings.header_image.width,
                                    widths: header_image_width_string
                                    }}	
                                {%- elsif block.settings.header_svg != blank -%}	
                                    {{ block.settings.header_svg }}	
                                {%- endif -%}
                            </div>
                        </div>
                        {% style %}
                            .block-{{ block.id }}-padding {
                                margin-top: {{ block.settings.padding_top_mobile | divided_by: 10.0 }}rem;
                                margin-bottom: {{ block.settings.padding_bottom_mobile | divided_by: 10.0 }}rem;
                            }

                            @media screen and (min-width: 750px) {
                                .block-{{ block.id }}-padding {
                                    margin-top: {{ block.settings.padding_top | divided_by: 10.0 }}rem;
                                    margin-bottom: {{ block.settings.padding_bottom | divided_by: 10.0 }}rem;
                                }
                            }

                            @media screen and (max-width: 749px) {
                                #shopify-section-{{ section.id }} .banner__image {
                                    width: {{ block.settings.header_image_width_mob }}px;
                                }
                            }

                            @media screen and (min-width: 750px) {
                                #shopify-section-{{ section.id }} .banner__image {
                                    width: {{ block.settings.header_image_width }}px;
                                }
                            }
                            #shopify-section-{{ section.id }} .banner__image {
                                color: {{ block.settings.svg_color }};
                            }
                        {% endstyle %}
                    {%- endcase -%}
			{%- endfor -%}
            {% if section.settings.link != blank %}
                <a href="{{ section.settings.link }}" {% if section.settings.link_new_tab %}target="_blank"{% endif %}><div class="banner__full-link"></div></a>
            {% endif %}
			</div>
		</div>
	</div>
</div>

{% schema %}
{
    "name": "t:sections.image-banner.name",
