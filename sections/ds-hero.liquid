{% comment %}
    Renders 'ds-hero' in sections everywhere
	previously 'image-banner'
{% endcomment %}

{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}

{%- if section.settings.adapt_height_first_image and section.settings.image_mob != blank -%}
	{%- style -%}
		@media screen and (max-width: 749px) {
			#Banner-{{ section.id }}::before,
			#Banner-{{ section.id }} .banner__media::before,
			#Banner-{{ section.id }}:not(.banner--mobile-bottom) .banner__content::before  {
				padding-bottom: {{ 1 | divided_by: section.settings.image_mob.aspect_ratio | times: 100 }}%;
				content: '';
				display: block;
			}
		}

		@media screen and (min-width: 750px) {
			#Banner-{{ section.id }}::before,
			#Banner-{{ section.id }} .banner__media::before {
				padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;
				content: '';
				display: block;
			}
		}
	{%- endstyle -%}
{%- elsif section.settings.adapt_height_first_image and section.settings.image != blank -%}
	{%- style -%}
		@media screen and (max-width: 749px) {
			#Banner-{{ section.id }}::before,
			#Banner-{{ section.id }} .banner__media::before,
			#Banner-{{ section.id }}:not(.banner--mobile-bottom) .banner__content::before {
				padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;
				content: '';
				display: block;
				}
			}

		@media screen and (min-width: 750px) {
			#Banner-{{ section.id }}::before,
			#Banner-{{ section.id }} .banner__media::before {
				padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;
				content: '';
				display: block;
			}
		}
	{%- endstyle -%}
{%- endif -%}

{%- style -%}
    #shopify-section-{{ section.id }} .banner__content {
        padding: unset;
    }

	.video-slide--background {
		-o-object-fit: cover;
		object-fit: cover;
		-o-object-position: center center;
		object-position: center center;
		width: 100%;
		display: block;
		height: 100%;
		position: absolute;
		top: 0;
		left: 0;
	}
    /* youtube / vimeo iframe settings - 16-9 aspect ratio will overflow */
    #shopify-section-{{ section.id }} .iframe-aspect-ratio iframe {
        position: absolute;
        width: 100%;
        height: 101%;
        top: 0;
        left: 0;
    }

    #shopify-section-{{ section.id }}  .iframe-aspect-ratio.iframe-aspect-ratio-16-9 {
        padding-top: calc(9 / 16* 100%);
        padding-left: calc(16 / 9* 100vh);
    }
    #shopify-section-{{ section.id }} .iframe-aspect-ratio {
        position: absolute;
        top: 50%;
        right: 50%;
        transform: translate(50%, -50%);
        width: 0;
        height: 0;
        min-height: 100%;
        min-width: 100%;
    }
    #shopify-section-{{ section.id }} .iframe-container {
        position: absolute!important;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        overflow: hidden;
        right: auto;
        max-width: 100%;
        min-width: 100%;
        max-height: 100%;
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center center;
    }

	.section-{{ section.id }}-padding {
		margin-top: {{ section.settings.padding_top_mobile }}px;
		margin-bottom: {{ section.settings.padding_bottom_mobile }}px;
	}

	@media screen and (min-width: 750px) {
		.section-{{ section.id }}-padding {
			margin-top: {{ section.settings.padding_top }}px;
			margin-bottom: {{ section.settings.padding_bottom }}px;
		}
	}

	:root:root #Banner-{{ section.id }}::after  {
		--color-base-background-1: {{ section.settings.overlay.red }}, {{ section.settings.overlay.green }}, {{ section.settings.overlay.blue }};
		--gradient-base-background-1: {% if section.settings.overlay_gradient != blank %}{{ section.settings.overlay_gradient }}{% else %}{{ section.settings.overlay }}{% endif %};
	}

	#Banner-{{ section.id }}::after {
		opacity: {{ section.settings.image_overlay_opacity | divided_by: 100.0 }};
		background: var(--gradient-base-background-1);
	}
	@media screen and (max-width: 749px) {
		#Banner-{{ section.id }}.banner.banner--mobile-bottom {
			--color-base-background-1: {{ section.settings.mob_below_overlay.red }}, {{ section.settings.mob_below_overlay.green }}, {{ section.settings.mob_below_overlay.blue }};
			--gradient-base-background-1: {% if section.settings.mob_below_overlay_gradient != blank %}{{ section.settings.mob_below_overlay_gradient }}{% else %}{{ section.settings.mob_below_overlay }}{% endif %};
			background: var(--gradient-base-background-1);
			background-color: var(--gradient-base-background-1);
		}
	}

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner--screen-height:not(.banner--adapt) {
            min-height: calc(100vh - var(--header-height));
        }
    }
    /* addded mobile content positioning */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner--xsmall:not(.banner--adapt) {
            min-height: unset;
        }

        #shopify-section-{{ section.id }} .banner--screen-height.banner--mobile-bottom:not(.banner--adapt) .banner__media {
            min-height: calc(100vh - var(--header-height));
        }

        #shopify-section-{{ section.id }} .banner--screen-height:not(.banner--mobile-bottom):not(.banner--adapt) .banner__content {
            min-height: calc(100vh - var(--header-height));
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            padding-top: {{ section.settings.banner__box_padding_top | divided_by: 10.0 }}rem;
            padding-bottom: {{ section.settings.banner__box_padding_bottom | divided_by: 10.0 }}rem;
            padding-left: {{ section.settings.banner__box_padding_left | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.banner__box_padding_right | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            padding-top: {{ section.settings.banner__box_padding_top_mob | divided_by: 10.0 }}rem;
            padding-bottom: {{ section.settings.banner__box_padding_bottom_mob | divided_by: 10.0 }}rem;
            padding-left: {{ section.settings.banner__box_padding_left_mob | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.banner__box_padding_right_mob | divided_by: 10.0 }}rem;
        }
    }
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            max-width: calc({{ section.settings.width_mob }}% - ({{ section.settings.mobile_content_position_left  | divided_by: 10.0 }}rem + {{ section.settings.mobile_content_position_right  | divided_by: 10.0 }}rem));
        }
    }
    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            max-width: calc({{ section.settings.width }}% - ({{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem + {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem));
        }
    }

    /* left */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-top-left .banner__box {
            left: {{ section.settings.mobile_content_position_left | divided_by: 10.0 }}rem;
            top: {{ section.settings.mobile_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-middle-left .banner__box  {
            left: {{ section.settings.mobile_content_position_left | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-bottom-left .banner__box  {
            bottom: {{ section.settings.mobile_content_position_bottom | divided_by: 10.0 }}rem;
            left: {{ section.settings.mobile_content_position_left | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--top-left .banner__box {
            left: {{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem;
            top: {{ section.settings.desktop_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--middle-left .banner__box  {
            left: {{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--bottom-left .banner__box  {
            bottom: {{ section.settings.desktop_content_position_bottom | divided_by: 10.0 }}rem;
            left: {{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem;
        }
    }

    /* right */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-top-right .banner__box {
            right: {{ section.settings.mobile_content_position_right | divided_by: 10.0 }}rem;
            top: {{ section.settings.mobile_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-middle-right .banner__box  {
            right: {{ section.settings.mobile_content_position_right | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-bottom-right .banner__box  {
            bottom: {{ section.settings.mobile_content_position_bottom | divided_by: 10.0 }}rem;
            right: {{ section.settings.mobile_content_position_right | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--top-right .banner__box {
            right: {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem;
            top: {{ section.settings.desktop_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--middle-right .banner__box {
            right: {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--bottom-right .banner__box {
            right: {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem;
            bottom: {{ section.settings.desktop_content_position_bottom | divided_by: 10.0 }}rem;
        }
    }

    /* center */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-top-center .banner__box {
            top: {{ section.settings.mobile_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-middle-center .banner__box {
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-bottom-center .banner__box {
            bottom: {{ section.settings.mobile_content_position_bottom | divided_by: 10.0 }}rem;
        }

        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--bottom-center .banner__box {
            bottom: {{ section.settings.desktop_content_position_bottom | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--middle-center .banner__box {
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--top-center .banner__box {
            top: {{ section.settings.desktop_content_position_top | divided_by: 10.0 }}rem;
        }

        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
        }
    }

    :root:root #Banner-{{ section.id }} .banner__box  {
		--color-base-background-1: {{ section.settings.banner_background.red }}, {{ section.settings.banner_background.green }}, {{ section.settings.banner_background.blue }};
		--gradient-base-background-1: {% if section.settings.banner_background_gradient != blank %}{{ section.settings.banner_background_gradient }}{% else %}{{ section.settings.banner_background }}{% endif %};
	}
{%- endstyle -%}

<div class="section-{{ section.id }}-margins {% if section.settings.page_width == 'normal' %} page-width-desktop-max-width-only{% endif %}">
	<div id="Banner-{{ section.id }}" class="banner banner--content-align-{{ section.settings.desktop_content_alignment }} banner--content-align-mobile-{{ section.settings.mobile_content_alignment }} banner--{{ section.settings.image_height }} {% if section.settings.adapt_height_first_image and section.settings.image != blank %} banner--adapt{% endif %}{% if section.settings.show_text_below %} banner--mobile-bottom{% endif %} section-{{ section.id }}-padding">
		{%- if section.settings.image != blank -%}
			<div class="banner__media media{% if section.settings.image == blank or section.settings.video_url == blank%} placeholder{% endif %}">
				{%- if section.settings.desktop_video_picker == blank and section.settings.video_url == blank -%}
					{%- if section.settings.image_mob != blank -%}
						<picture>
							<source srcset="{%- if section.settings.image_mob.width >= 375 -%}{{ section.settings.image_mob | image_url: width: 375  }} 375w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 550 -%}{{ section.settings.image_mob | image_url: width: 550 }} 550w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 750 -%}{{ section.settings.image_mob | image_url: width: 750 }} 750w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 1100 -%}{{ section.settings.image_mob | image_url: width: 1100 }} 1100w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 1500 -%}{{ section.settings.image_mob | image_url: width: 1500 }} 1500w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 1780 -%}{{ section.settings.image_mob | image_url: width: 1780 }} 1780w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 2000 -%}{{ section.settings.image_mob | image_url: width: 2000 }} 2000w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 3000 -%}{{ section.settings.image_mob | image_url: width: 3000 }} 3000w,{%- endif -%}
									{%- if section.settings.image_mob.width >= 3840 -%}{{ section.settings.image_mob | image_url: width: 3840 }} 3840w,{%- endif -%}
									{{ section.settings.image_mob | image_url }} {{ section.settings.image_mob.width }}w"
                                    width="{{ section.settings.image_mob.width }}"
                                    height="{{ section.settings.image_mob.height }}"
									media="(max-width: 749px)"
									sizes='100vw'
								>
                            <source srcset="{%- if section.settings.image.width >= 375 -%}{{ section.settings.image | image_url: width: 375 }} 375w,{%- endif -%}
                                {%- if section.settings.image.width >= 550 -%}{{ section.settings.image | image_url: width: 550  }} 550w,{%- endif -%}
                                {%- if section.settings.image.width >= 750 -%}{{ section.settings.image | image_url: width: 750 }} 750w,{%- endif -%}
                                {%- if section.settings.image.width >= 1100 -%}{{ section.settings.image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                {%- if section.settings.image.width >= 1500 -%}{{ section.settings.image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                {%- if section.settings.image.width >= 1780 -%}{{ section.settings.image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                {%- if section.settings.image.width >= 2000 -%}{{ section.settings.image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                {%- if section.settings.image.width >= 3000 -%}{{ section.settings.image | image_url: width: 3000 }} 3000w,{%- endif -%}
                                {%- if section.settings.image.width >= 3840 -%}{{ section.settings.image | image_url: width: 3840 }} 3840w,{%- endif -%}
                                {{ section.settings.image | image_url }} {{ section.settings.image.width }}w"
                                media="(min-width: 750px)"
                                width="{{ section.settings.image.width }}"
                                height="{{ section.settings.image.height }}"
                                sizes="100vw"
                            >
                            {%- capture hero_height -%}{{section.settings.image.width | divided_by: section.settings.image.aspect_ratio }}{%- endcapture -%}
                            {% if section.settings.lazy_load == true %}
                            {{ section.settings.image | image_url: width: 1920 | image_tag:
                                loading: 'lazy',
                                height: hero_height,
                                widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840' }}
                            {% else %}
                            {{ section.settings.image | image_url: width: 1920 | image_tag:
                                loading: 'eager',
                                preload: true,
                                height: hero_height,
                                widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840' }}
                            {% endif %}
						</picture>
					{%- else -%}
                    {%- capture sizes -%}100vw{%- endcapture -%}
                    {%- capture image_height %}{{ section.settings.image.width | divided_by: section.settings.image.aspect_ratio }}{%- endcapture -%}
                        {% if section.settings.lazy_load == true %}
                        {{ section.settings.image | image_url: width: 1920 | image_tag:
                            loading: 'lazy',
                            widths: '750, 1100, 1500, 1780, 2000, 3000, 3840',
                            height: image_height,
                            sizes: sizes }}
                        {% else %}
                        {{ section.settings.image | image_url: width: 1920 | image_tag:
                            loading: 'eager',
                            preload: true,
                            widths: '750, 1100, 1500, 1780, 2000, 3000, 3840',
                            height: image_height,
                            sizes: sizes }}
                        {% endif %}
					{%- endif -%}
				{%- else -%}
                    {% if section.settings.desktop_video_picker != blank %}
                        {%- capture desktop_src -%}{% for source in section.settings.desktop_video_picker.sources %}{% if source.format == 'mp4' %}{{ source.url }}{% endif %}{% endfor %}{%- endcapture -%}
                        {%- capture mobile_src -%}{% for source in section.settings.mobile_video_picker.sources %}{% if source.format == 'mp4' %}{{ source.url }}{% endif %}{% endfor %}{%- endcapture -%}
                            <video-banner data-desktop-src="{{ desktop_src }}"
                                data-desktop-poster="{{ section.settings.image | image_url: width: section.settings.image.width }}" data-mobile-src="{{ mobile_src }}"
                                data-mobile-poster="{{ section.settings.image_mob | image_url: width: section.settings.image_mob.width }}">
                                <video src="{{ desktop_src }}" src="{{ mobile_src }}" id="hero-video"
                                    class="video-slide--background video-control-button--target" loop autoplay playsinline loop muted></video>
                            </video-banner>
                            <div class="video-control-button__container">
                                {% render 'video-control-button' %}
                            </div>
                    {%- elsif section.settings.video_url != blank -%}
                        {%- if section.settings.video_url.type == 'youtube' -%}
                            <div class="iframe-container">
                                <div class="iframe-aspect-ratio iframe-aspect-ratio-16-9">
                                    {% comment %} <iframe src="https://www.youtube.com/embed/{{ section.settings.video_url.id }}?enablejsapi=1" class="js-youtube" allow="autoplay; encrypted-media" allowfullscreen title="{{ section.settings.description | escape }}"></iframe> {% endcomment %}
                                        <iframe title="{{ section.settings.title | escape }}" src="https://www.youtube.com/embed/{{ section.settings.video_url.id }}?playsinline=1&autoplay=1&controls=0&mute=1&loop=1&playlist={{ section.settings.video_url.id }}&enablejsapi=1&rel=0&modestbranding=1&origin={{ 'https://' | append: request.host | url_encode }}" allow="autoplay; encrypted-media" allowfullscreen="allowfullscreen"></iframe>
                                </div>
                            </div>
                            {%- else -%}
                            <div class="iframe-container">
                                <div class="iframe-aspect-ratio iframe-aspect-ratio-16-9">
                                    {% comment %} <iframe src="https://player.vimeo.com/video/{{ section.settings.video_url.id }}" class="js-vimeo" allow="autoplay; encrypted-media" allowfullscreen title="{{ section.settings.description | escape }}"></iframe> {% endcomment %}
                                    <iframe title="{{ section.settings.title | escape }}" src="https://player.vimeo.com/video/{{ section.settings.video_url.id }}?autoplay=1&autopause=1&background=1&loop=1&muted=1&transparent=0&responsive=1&portrait=0&title=0&byline=0&color={{ '#ffffff' | remove_first: '#' }}" allow="autoplay; encrypted-media;" allowfullscreen="allowfullscreen"></iframe>
                                </div>
                            </div>
                            {%- endif -%}
                        {% endif %}
				{%- endif -%}
			</div>
		{%- else -%}
			<div class="banner__media media{% if section.settings.image == blank %} placeholder{% endif %}">
				{{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
			</div>
		{%- endif -%}
        {% if section.settings.link != blank %}
            <a href="{{ section.settings.link }}" {% if section.settings.link_new_tab %}target="_blank"{% endif %}><div class="banner__full-link"></div></a>
        {% endif %}

		<div class="banner__content banner__content--{{ section.settings.desktop_content_position }} banner__content--mobile-{{ section.settings.mobile_content_position }}{% if section.settings.banner_content_page_width %} page-width-desktop-max-width-only{% endif %}">
			<div class="banner__box color-background-1 gradient">
			{%- for block in section.blocks -%}
				{%- case block.type -%}
					{%- when 'heading' -%}
						{% style %}
                            #shopify-section-{{ section.id }} .banner .banner__heading {
                                display: flex;
                                align-items: center;
                                gap: 1rem;
                            }

							@media screen and (max-width: 749px) {
								#shopify-section-{{ section.id }} .banner.banner--mobile-bottom .banner__heading {
									color: {{ block.settings.heading_below_color }};
								}
								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__heading {
									color: {{ block.settings.heading_color }};
								}
							}
							@media screen and (min-width: 750px) {
								#shopify-section-{{ section.id }} .banner .banner__heading {
									color: {{ block.settings.heading_color }};
								}
							}

                            #shopify-section-{{ section.id }} .banner .banner__heading .icon {
                                flex-shrink: 0;
                            }
                            
                            /* Desktop custom heading */
                            @media screen and (min-width: 1024px) {
                                #shopify-section-{{ section.id }} .banner__heading.custom-desktop {
                                    font-size: {{ block.settings.heading_size_custom | divided_by: 10.0 }}rem;
                                }

                                #shopify-section-{{ section.id }} .banner__heading .icon {
                                    width: {{ block.settings.heading_size_custom | divided_by: 10.0 }}rem;
                                    height: {{ block.settings.heading_size_custom | divided_by: 10.0 }}rem;
                                }
                            }
                            /* Mobile custom heading */
                            @media screen and (max-width: 1023px) {
                                #shopify-section-{{ section.id }} .banner__heading.custom-mobile {
                                    font-size: {{ block.settings.heading_size_mobile_custom | divided_by: 10.0 }}rem;
                                }

                                #shopify-section-{{ section.id }} .banner__heading .icon {
                                    width: {{ block.settings.heading_size_mobile_custom | divided_by: 10.0 }}rem;
                                    height: {{ block.settings.heading_size_mobile_custom | divided_by: 10.0 }}rem;
                                }
                            }

                            #shopify-section-{{ section.id }} .block-{{ block.id }}-padding {
                                margin-top: {{ block.settings.padding_top_mobile | divided_by: 10.0 }}rem;
