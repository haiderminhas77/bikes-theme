                                        :root:root  #shopify-section-{{ section.id }} .banner.banner--mobile-bottom .banner__caption-{{ block.id }} a {
                                            color: {{ block.settings.caption_link_below_color }};
                                        }
                                    }

                                    @media screen and (min-width: 750px) {
                                        :root:root  #shopify-section-{{ section.id }} .banner__caption-{{ block.id }} {
                                            --color-base-text: {{ block.settings.caption_text_color.red }}, {{ block.settings.caption_text_color.green }}, {{ block.settings.caption_text_color.blue }};
                                        }

                                        :root:root  #shopify-section-{{ section.id }} .banner__caption-{{ block.id }} a {
                                            color: {{ block.settings.caption_link_color }};
                                        }
                                    }

                                    #shopify-section-{{ section.id }} .banner__caption p {
                                        margin: unset;
                                        padding: unset;
                                    }

                                    /* Desktop custom font size */
                                    @media screen and (min-width: 750px) {
                                        #shopify-section-{{ section.id }} .banner__caption-{{ block.id }}.custom-desktop {
                                            font-size: {{ block.settings.caption_size_custom | divided_by: 10.0 }}rem;
                                        }
                                    }
                                    /* Mobile custom font size */
                                    @media screen and (max-width: 749px) {
                                        #shopify-section-{{ section.id }} .banner__caption-{{ block.id }}.custom-mobile {
                                            font-size: {{ block.settings.caption_size_mobile_custom | divided_by: 10.0 }}rem;
                                        }
                                    }

                                    #shopify-section-{{ section.id }} .banner__caption-{{ block.id }} {
                                        font-family: var(--font-caption-family);
                                        line-height: 0.9;
                                        font-style: var(--font-caption-style);
                                        {% if block.settings.caption_font_weight == 'bold' %}
                                            font-weight: var(--font-caption-weight);
                                        {% elsif block.settings.caption_font_weight == 'light' %}
                                            font-weight: var(--font-caption-weight-light);
                                        {% else %}
                                            font-weight: var(--font-caption-weight);
                                        {% endif %}
                                        {% if block.settings.caption_letter_spacing == 'wide' %}
                                            letter-spacing: var(--font-caption-letter-spacing-wide);
                                        {% endif %}
                                    }

                                    #shopify-section-{{ section.id }} .banner_caption-{{ block.id }}-padding {
                                        margin-top: {{ block.settings.caption_padding_top_mobile }}px!important;
                                        margin-bottom: {{ block.settings.caption_padding_bottom_mobile }}px!important;
                                    }

                                    @media screen and (min-width: 750px) {
                                        #shopify-section-{{ section.id }} .banner_caption-{{ block.id }}-padding {
                                            margin-top: {{ block.settings.caption_padding_top }}px!important;
                                            margin-bottom: {{ block.settings.caption_padding_bottom }}px!important;
                                        }
                                    }
                                {% endstyle %}
                                <div class="banner__caption banner__caption-{{ block.id }} {{ block.settings.caption_size }}-desktop {{ block.settings.caption_size_mobile }}-mobile{% if block.settings.caption_uppercase %} uppercase{% endif %} banner_caption-{{ block.id }}-padding">{{ block.settings.caption }}</div>
                            {%- endif -%}
                            {%- if block.settings.heading != blank -%}
                                {% style %}
                                /* Desktop custom heading*/
                                @media screen and (min-width: 1201px) {
                                    #shopify-section-{{ section.id }} .banner_heading-{{ block.id }}.custom-desktop {
                                        font-size: {{ block.settings.heading_size_custom | divided_by: 10.0 }}rem;
                                    }
                                }

                                /* tablet custom heading */
                                @media screen and (min-width: 750px) and (max-width: 1200px){
                                    #shopify-section-{{ section.id }} .banner_heading-{{ block.id }} {
                                        font-size: {{ block.settings.heading_size_tablet_custom | divided_by: 10.0 }}rem;
                                    }
                                }
                                /* Mobile custom heading */
                                @media screen and (max-width: 749px) {
                                    #shopify-section-{{ section.id }} .banner_heading-{{ block.id }}.custom-mobile {
                                        font-size: {{ block.settings.heading_size_mobile_custom | divided_by: 10.0 }}rem;
                                    }
                                }

                                #shopify-section-{{ section.id }} .banner_heading-{{ block.id }}-padding {
                                    margin-top: {{ block.settings.heading_padding_top_mobile }}px!important;
                                    margin-bottom: {{ block.settings.heading_padding_bottom_mobile }}px!important;
                                }

                                @media screen and (min-width: 750px) {
                                    #shopify-section-{{ section.id }} .banner_heading-{{ block.id }}-padding {
                                        margin-top: {{ block.settings.heading_padding_top }}px!important;
                                        margin-bottom: {{ block.settings.heading_padding_bottom }}px!important;
                                    }
                                }

                                #shopify-section-{{ section.id }} .banner__heading-{{ block.id }} {
                                    font-family: var(--font-heading-family);
                                    line-height: 0.9;
                                    letter-spacing: var(--font-heading-letter-spacing-narrow);
                                }
                                {% endstyle %}
                                <h2 class="banner__heading banner__heading-{{ block.id }} {{ block.settings.heading_size }}-desktop {{ block.settings.heading_size_mobile }}-mobile  {% if block.settings.heading_uppercase %}uppercase {% endif %} banner_heading-{{ block.id }} banner_heading-{{ block.id }}-padding">{{ block.settings.heading }}</h2>
                            {%- endif -%}
                            {%- if block.settings.text != blank -%}
                                {% style %}
                                /* Desktop custom font size */
                                @media screen and (min-width: 750px) {
                                    #shopify-section-{{ section.id }} .banner__text-{{ block.id }}.custom-desktop {
                                        font-size: {{ block.settings.text_size_custom | divided_by: 10.0 }}rem;
                                    }
                                }
                                /* Mobile custom font size */
                                @media screen and (max-width: 749px) {
                                    #shopify-section-{{ section.id }} .banner__text-{{ block.id }}.custom-mobile {
                                        font-size: {{ block.settings.text_size_mobile_custom | divided_by: 10.0 }}rem;
                                    }
                                }

                                #shopify-section-{{ section.id }} .banner__text-{{ block.id }}-padding {
                                    margin-top: {{ block.settings.text_padding_top_mobile }}px!important;
                                    margin-bottom: {{ block.settings.text_padding_bottom_mobile }}px!important;
                                }

                                @media screen and (min-width: 750px) {
                                    #shopify-section-{{ section.id }} .banner__text-{{ block.id }}-padding {
                                        margin-top: {{ block.settings.text_padding_top }}px!important;
                                        margin-bottom: {{ block.settings.text_padding_bottom }}px!important;
                                    }
                                }
                                {% if block.settings.text_font_family == 'body' %}
                                        #shopify-section-{{ section.id }} .banner__text-{{ block.id }} {
                                            font-family:  var(--font-body-family);
                                            line-height: 1.3;
                                        }
                                    {% else %}
                                        #shopify-section-{{ section.id }} .banner__text-{{ block.id }} {
                                            font-family: var(--font-caption-family);
                                            line-height: 1.1;
                                            font-style: var(--font-caption-style);
                                            font-weight: var(--font-caption-weight);
                                            letter-spacing: var(--font-caption-letter-spacing-wide);
                                        }
                                    {% endif %}
                                {% endstyle %}

                                <div class="banner__text banner__text-{{ block.id }} {{ block.settings.text_size }}-desktop {{ block.settings.text_size_mobile }}-mobile{% if block.settings.text_uppercase %} uppercase{% endif %} banner__text-{{ block.id }}-padding">
                                    {{ block.settings.text }}
                                </div>
                            {%- endif -%}
                            {%- if block.settings.button_label_1 != blank -%}
                                {% style %}
                                 /* Desktop custom font size */
                                    @media only screen and (min-width: 750px) {
                                        #shopify-section-{{ section.id }} .banner .banner__buttons-{{ block.id }} .custom-desktop {
                                            font-size: {{ block.settings.button_size_custom | divided_by: 10.0 }}rem;
                                        }
                                        #shopify-section-{{ section.id }} .banner__buttons-{{ block.id }} {
                                            gap: {{ block.settings.buttons_gap }}rem;
                                        }
                                    }
                                    /* Mobile custom font size */
                                    @media screen and (max-width: 749px) {
                                        #shopify-section-{{ section.id }} .banner .banner__buttons-{{ block.id }} .custom-mobile {
                                            font-size: {{ block.settings.button_size_mobile_custom | divided_by: 10.0 }}rem;
                                        }
                                        #shopify-section-{{ section.id }} .banner__buttons-{{ block.id }} {
                                            gap: {{ block.settings.buttons_gap_mob }}rem;
                                        }
                                    }

                                    #shopify-section-{{ section.id }} .banner_buttons-{{ block.id }}-padding {
                                        margin-top: {{ block.settings.buttons_padding_top_mobile }}px!important;
                                        margin-bottom: {{ block.settings.buttons_padding_bottom_mobile }}px!important;
                                    }

                                    @media screen and (min-width: 750px) {
                                        #shopify-section-{{ section.id }} .banner_buttons-{{ block.id }}-padding {
                                            margin-top: {{ block.settings.buttons_padding_top }}px!important;
                                            margin-bottom: {{ block.settings.buttons_padding_bottom }}px!important;
                                        }
                                    }
                                {% endstyle %}
                                <div class="banner__buttons banner__buttons-{{ block.id }} banner_buttons-{{ block.id }}-padding {% if block.settings.button_label_1 != blank and block.settings.button_label_2 != blank %} banner__buttons--multiple{% endif %} block-{{ block.id }}-padding{% if block.settings.buttons_mobile_position == 'stacked' %} banner__buttons--stacked-mobile{% elsif block.settings.buttons_mobile_position == 'full_width' %} banner__buttons--full-width{% endif %}" {{ block.shopify_attributes }}>
                                    {%- if block.settings.button_label_1 != blank -%}
                                        <a{% if block.settings.button_link_1 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link_1 }}"{% endif %} {% if block.settings.button_new_tab_1 %}target="_blank"{% endif %} class="{% if block.settings.button_style_1 != 'arrow' %}button{% endif %}{% if block.settings.button_style_1 == 'arrow' %} link link-with-icon{% endif %}{% if block.settings.button_style_1 == 'outline' %} button--secondary{% elsif block.settings.button_style_1 == 'primary' %} button--primary{% elsif block.settings.button_style_1 == 'underline' %} button--underline{% endif %}{% if block.settings.button_uppercase_1 %} uppercase{% endif %} {{ block.settings.button_size }}-desktop {{ block.settings.button_size_mobile }}-mobile{% if block.settings.button_wide_1 %} button--wide{% endif %}">
                                            {{ block.settings.button_label_1 | escape }}{% if block.settings.button_style_1 == 'arrow'%}{%- render 'icon-arrow-right' -%}{% endif %}
                                        </a>
                                    {%- endif -%}
                                    {%- if block.settings.button_label_2 != blank -%}
                                        <a{% if block.settings.button_link_2 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link_2 }}"{% endif %} {% if block.settings.button_new_tab_2 %}target="_blank"{% endif %} class="{% if block.settings.button_style_2 != 'arrow' %}button{% endif %}{% if block.settings.button_style_2 == 'arrow' %} link link-with-icon{% endif %}{% if block.settings.button_style_2 == 'outline' %} button--secondary{% elsif block.settings.button_style_2 == 'primary' %} button--primary{% elsif block.settings.button_style_2 == 'underline' %} button--underline{% endif %}{% if block.settings.button_uppercase_2 %} uppercase{% endif %} {{ block.settings.button_size }}-desktop {{ block.settings.button_size_mobile }}-mobile{% if block.settings.button_wide_2 %} button--wide{% endif %}">
                                            {{ block.settings.button_label_2 | escape }}{% if block.settings.button_style_2 == 'arrow'%}{%- render 'icon-arrow-right' -%}{% endif %}</a>
                                    {%- endif -%}
                                </div>
                            {%- endif -%}
                            {% if block.settings.link != blank %}
                                <a href="{{ block.settings.link }}" {% if block.settings.link_new_tab %}target="_blank"{% endif %}><div class="banner__full-link"></div></a>
                            {% endif %}
                        </div>
                    </div>
                </div>
            {%- endfor -%}
        </div>

        {%- if section.blocks.size > 1 -%}
        <div class="slider-buttons no-js-hidden">
            <div class="slideshow__controls">
                <button type="button" class="slider-button slider-button--prev" name="previous" aria-label="{{ 'sections.slideshow.previous_slideshow' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-arrow-left' %}</button>
                <button type="button" class="slider-button slider-button--next" name="next" aria-label="{{ 'sections.slideshow.next_slideshow' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-arrow-right' %}</button>
            </div>
            <div class="slider-counter slider-counter--{{ section.settings.slider_visual }}{% if section.settings.slider_visual == 'counter' or section.settings.slider_visual == 'numbers' %} caption{% endif %}" {% if section.settings.slider_visual == 'hidden' %}style="display: none;"{% endif %}>
                {%- if section.settings.slider_visual == 'counter' -%}
                    <span class="slider-counter--current">1</span>
                    <span aria-hidden="true"> / </span>
                    <span class="visually-hidden">{{ 'general.slider.of' | t }}</span>
                    <span class="slider-counter--total">{{ section.blocks.size }}</span>
                {%- else-%}
                    <span class="slider-counter--total slider-counter--current visually-hidden">{{ section.blocks.size }}</span>
                    <div class="slideshow__control-wrapper">
                    {%- for block in section.blocks -%}
                        <button class="slider-counter__link slider-counter__link--{{ section.settings.slider_visual }} link" aria-label="{{ 'sections.slideshow.load_slide' | t }} {{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length}}" aria-controls="Slider-{{ section.id }}" {{ block.shopify_attributes }}>
                            {%- if section.settings.slider_visual == 'numbers' -%}{{ forloop.index }}{% else %}<span class="dot"></span>{%- endif -%}
                        </button>
                    {%- endfor -%}
                    </div>
                {%- endif -%}
            </div>
        </div>
        {%- if section.settings.auto_rotate -%}
            <button type="button" class="slideshow__autoplay slider-button no-js-hidden{% if section.settings.auto_rotate == false %} slideshow__autoplay--paused{% endif %}" aria-label="{{ 'sections.slideshow.pause_slideshow' | t }}">
                {%- render 'icon-pause' -%}
                {%- render 'icon-play-without-bg' -%}
            </button>
        {%- endif -%}
        <noscript>
            <div class="slider-buttons">
                <div class="slider-counter">
                    {%- for block in section.blocks -%}
                        <a href="#Slide-{{ section.id }}-{{ forloop.index }}" class="slider-counter__link link" aria-label="{{ 'sections.slideshow.load_slide' | t }} {{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length}}" {{ block.shopify_attributes }}>
                            {{ forloop.index }}
                        </a>
                    {%- endfor -%}
                </div>
            </div>
        </noscript>
        {%- endif -%}
    </slideshow-component>
</div>

{%- if request.design_mode -%}
	<script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

{% schema %}
{
    "name": "t:sections.slideshow.name",
    "tag": "section",
    "class": "section",
    "disabled_on": {
        "groups": ["header", "footer"]
    },
    "settings": [
        {
            "type": "paragraph",
            "content": "Read the DS Slideshow GitBook documentation [here](https://app.gitbook.com/o/HciOHRqvd32zv1ZdkdeF/s/xztZa7d0pkVT15plvtPU/dynamic-section/dynamic-sections#ds-slideshow)"
        },
        {
            "type": "header",
            "content": "Section Width"
        },
        {
            "type": "select",
            "id": "page_width",
            "options": [
                {
                    "value": "normal",
                    "label": "normal"
                },
                {
                    "value": "full",
                    "label": "full"
                }
            ],
            "default": "full",
            "label": "Add a set page width to desktop"
        },
        {
            "type": "range",
            "id": "margin_left_right_desktop",
            "min": 0,
            "max": 40,
            "step": 1,
            "default": 0,
            "unit": "px",
            "label": "Margin left/right"
        },
        {
            "type": "range",
            "id": "margin_left_right_mob",
            "min": 0,
            "max": 40,
            "step": 1,
            "default": 0,
            "unit": "px",
            "label": "Margin left/right mob"
        },
        {
            "type": "header",
            "content": "Slides"
        },
        {
            "type": "checkbox",
            "id": "lazy_load",
            "default": true,
            "label": "Enable lazy load"
        },
        {
            "type": "select",
            "id": "slide_height",
            "options": [
                {
                    "value": "adapt_image",
                    "label": "t:sections.slideshow.settings.slide_height.options__1.label"
                },
                {
                    "value": "small",
                    "label": "t:sections.slideshow.settings.slide_height.options__2.label"
                },
                {
                    "value": "medium",
                    "label": "t:sections.slideshow.settings.slide_height.options__3.label"
                },
                {
                    "value": "large",
                    "label": "t:sections.slideshow.settings.slide_height.options__4.label"
                },
                {
                    "value": "screen-height",
                    "label": "Screen height"
                }
            ],
            "default": "adapt_image",
            "label": "t:sections.slideshow.settings.slide_height.label"
        },
        {
            "type": "select",
            "id": "slider_visual",
            "options": [
                {
                    "value": "dots",
                    "label": "t:sections.slideshow.settings.slider_visual.options__2.label"
                },
                {
                    "value": "counter",
                    "label": "t:sections.slideshow.settings.slider_visual.options__1.label"
                },
                {
                    "value": "numbers",
                    "label": "t:sections.slideshow.settings.slider_visual.options__3.label"
                },
                {
                    "value": "hidden",
                    "label": "Hidden"
                }
            ],
            "default": "hidden",
            "label": "Pagination style"
        },
        {
            "type": "checkbox",
            "id": "auto_rotate",
            "label": "t:sections.slideshow.settings.auto_rotate.label",
            "default": false
        },
        {
            "type": "range",
            "id": "change_slides_speed",
            "min": 3,
            "max": 9,
            "step": 2,
            "unit": "s",
            "label": "t:sections.slideshow.settings.change_slides_speed.label",
            "default": 5
        },
        {
            "type": "color",
            "id": "slider_arrows",
            "default": "#ffffff",
            "label": "Arrows"
        },
        {
            "type": "color",
            "id": "slider_arrows_background",
            "default": "#4d4d4d",
            "label": "Arrows background"
        },
        {
            "type": "select",
            "id": "arrow_position",
            "options": [
                {
                    "value": "left",
                    "label": "Left"
                },
                {
                    "value": "center",
                    "label": "Center"
                },
                {
                    "value": "right",
                    "label": "Right"
                },
                {
                    "value": "space-between",
                    "label": "Space Between"
                }
            ],
            "default": "space-between",
            "label": "Arrows positioning"
        },
        {
            "type": "range",
            "id": "arrow_padding_left_right",
            "min": 0,
            "max": 100,
            "step": 2,
            "unit": "px",
            "label": "Arrows padding left/right [desktop]",
            "default": 14
        },
        {
            "type": "range",
            "id": "arrow_padding_left_right_mobile",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "px",
            "label": "Arrows padding left/right [mob]",
            "default": 14
        },
        {
            "type": "header",
            "content": "t:sections.slideshow.settings.mobile.content"
        },
        {
            "type": "checkbox",
            "id": "show_text_below",
            "label": "t:sections.slideshow.settings.show_text_below.label",
            "default": false
        },
        {
            "type": "header",
            "content": "t:sections.slideshow.settings.accessibility.content"
        },
        {
            "type": "text",
            "id": "accessibility_info",
            "label": "t:sections.slideshow.settings.accessibility.label",
            "info": "t:sections.slideshow.settings.accessibility.info",
            "default": "Slideshow about our brand"
        },
        {
            "type": "header",
            "content": "t:sections.all.padding.section_padding_heading"
        },
        {
            "type": "range",
            "id": "padding_top",
            "min": 0,
            "max": 160,
            "step": 2,
            "unit": "px",
            "label": "t:sections.all.padding.padding_top",
            "default": 0
        },
        {
            "type": "range",
            "id": "padding_bottom",
            "min": 0,
            "max": 160,
            "step": 2,
            "unit": "px",
            "label": "t:sections.all.padding.padding_bottom",
            "default": 20
        },
        {
            "type": "range",
            "id": "padding_top_mobile",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "px",
            "label": "t:sections.all.padding.padding_top_mobile",
            "default": 8
        },
        {
            "type": "range",
            "id": "padding_bottom_mobile",
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "px",
            "label": "t:sections.all.padding.padding_bottom_mobile",
            "default": 8
        }
    ],
    "blocks": [
        {
            "type": "slide",
            "name": "t:sections.slideshow.blocks.slide.name",
            "limit": 5,
            "settings": [
                {
                    "type": "header",
                    "content": "Image"
                },
                {
                    "type": "image_picker",
                    "id": "image",
                    "label": "Desktop image"
                },
                {
                    "type": "image_picker",
                    "id": "image_mob",
                    "label": "Mobile image"
                },
                {
                    "type": "url",
                    "id": "link",
                    "label": "Image link"
                },
                {
                    "type": "checkbox",
                    "id": "link_new_tab",
                    "default": false,
                    "label": "Open link in new tab"
                },
                {
                    "type": "color",
                    "id": "overlay",
                    "default": "#1c1c1c",
                    "label": "Background"
                },
                {
                    "id": "overlay_gradient",
                    "type": "color_background",
                    "label": "Background gradient"
                },
                {
                    "type": "range",
                    "id": "image_overlay_opacity",
                    "min": 0,
                    "max": 100,
                    "step": 10,
                    "unit": "%",
                    "label": "t:sections.slideshow.blocks.slide.settings.image_overlay_opacity.label",
                    "default": 10
                },
                {
                    "type": "image_picker",
                    "id": "header_image",
                    "label": "Extra image"
                },
                {
                    "type": "range",
                    "id": "header_image_width",
                    "min": 50,
                    "max": 300,
                    "step": 5,
                    "unit": "px",
                    "label": "Image width",
                    "default": 240
                },
                {
                    "type": "range",
                    "id": "header_image_width_mob",
                    "min": 50,
                    "max": 300,
                    "step": 5,
                    "unit": "px",
                    "label": "Image width mob",
                    "default": 145
                },
                {
                    "type": "html",
                    "id": "header_svg",
                    "label": "SVG logo",
                    "info": "Add an SVG html element, image must be removed to show SVG"
                },
                {
                    "type": "color",
                    "id": "svg_color",
                    "default": "#ffffff",
                    "label": "SVG colour"
                },
                {
                    "type": "header",
                    "content": "If Text Below",
                    "info": "Only on mob"
                },
                {
                    "type": "color",
                    "id": "mob_below_overlay",
                    "default": "#CECECE",
                    "label": "Text below background"
                },
                {
                    "id": "mob_below_overlay_gradient",
                    "type": "color_background",
                    "label": "Text below background gradient"
                },
                {
                    "type": "header",
                    "content": "Caption"
                },
                {
                    "type":  "richtext",
