{% comment %}
	Renders 'ds-slideshow' in sections everywhere
{% endcomment %}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}
{{ 'component-slideshow.css' | asset_url | stylesheet_tag }}
{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}

{%- if section.settings.slide_height == 'adapt_image' and section.blocks.first.settings.image_mob != blank -%}
	{%- style -%}
        @media screen and (max-width: 749px) {
            #Slider-{{ section.id }}::before,
            #Slider-{{ section.id }} .media::before,
            #Slider-{{ section.id }}:not(.banner--mobile-bottom) .banner__content::before {
                padding-bottom: {{ 1 | divided_by: section.blocks.first.settings.image_mob.aspect_ratio | times: 100 }}%;
                content: '';
                display: block;
            }
        }

        @media screen and (min-width: 750px) {
            #Slider-{{ section.id }}::before,
            #Slider-{{ section.id }} .media::before {
                padding-bottom: {{ 1 | divided_by: section.blocks.first.settings.image.aspect_ratio | times: 100 }}%;
                content: '';
                display: block;
            }
        }
	{%- endstyle -%}
{%- elsif section.settings.slide_height == 'adapt_image' and section.blocks.first.settings.image != blank -%}
    {%- style -%}
        @media screen and (max-width: 749px) {
            #Slider-{{ section.id }}::before,
            #Slider-{{ section.id }} .media::before,
            #Slider-{{ section.id }}:not(.banner--mobile-bottom) .banner__content::before {
                padding-bottom: {{ 1 | divided_by: section.blocks.first.settings.image.aspect_ratio | times: 100 }}%;
                content: '';
                display: block;
            }
        }

        @media screen and (min-width: 750px) {
            #Slider-{{ section.id }}::before,
            #Slider-{{ section.id }} .media::before {
                padding-bottom: {{ 1 | divided_by: section.blocks.first.settings.image.aspect_ratio | times: 100 }}%;
                content: '';
                display: block;
            }
        }
    {%- endstyle -%}
{%- endif -%}

{%- style -%}
    #shopify-section-{{ section.id }} .slider-buttons .slideshow__controls {
        position: absolute;
        width: 100%;
        left: 0;
        bottom: 4rem;
        justify-content: {{ section.settings.arrow_position }};
        /* z-index: 2; */
        border: none;
        margin: 0px;
        gap: 0px 20px;
        display: flex;
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .slider-buttons .slideshow__controls {
            padding: 0 {{ section.settings.arrow_padding_left_right }}px;
        }
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .slider-buttons .slideshow__controls {
            padding: 0 {{ section.settings.arrow_padding_left_right_mobile }}px;
        }
    }
    
    #shopify-section-{{ section.id }} .slider-button {
        z-index: 2;
    }

    #shopify-section-{{ section.id }} .slideshow__autoplay.slider-button {
        padding: 1rem;
        margin: 0px 1.4rem 0 0;
        position: absolute;
        bottom: 1rem;
        background: transparent;
        z-index: 2;
        border: none;
    }

	.section-{{ section.id }}-padding {
		margin-top: {{ section.settings.padding_top_mobile }}px;
		margin-bottom: {{ section.settings.padding_bottom_mobile }}px;
	}

	@media screen and (min-width: 750px) {
		.section-{{ section.id }}-padding {
			margin-top: {{ section.settings.padding_top }}px;
			margin-bottom: {{ section.settings.padding_bottom }}px;
		}
        
        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
        }
	}
    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner--screen-height {
            min-height: calc(100vh - var(--header-height));
        }
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner--mobile-bottom .slider__slide {   
            height: 100%;   
        }

        #shopify-section-{{ section.id }} .banner--screen-height.banner--mobile-bottom .banner__media {
            min-height: calc(100vh - var(--header-height));
        }

        #shopify-section-{{ section.id }} .banner--screen-height:not(.banner--mobile-bottom) .banner__content {
            min-height: calc(100vh - var(--header-height));
        }

        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
        }
    }
    #shopify-section-{{ section.id }} .banner__heading p {
        margin: 0;
        padding: 0;
    }

    #shopify-section-{{ section.id }} .banner__text p {
        margin: 0;
        padding: 0;
    }

    /* progress slider colour settings */
    #shopify-section-{{ section.id }} .progress-slider .slider-button {
        --color-base-background-2: {{ section.settings.slider_arrows_background.red }},  {{ section.settings.slider_arrows_background.green }}, {{ section.settings.slider_arrows_background.blue }};
        --color-base-background-1: {{ section.settings.slider_arrows.red }},  {{ section.settings.slider_arrows.green }}, {{ section.settings.slider_arrows.blue }};
        padding: 1rem;
    }
{%- endstyle -%}


<div class="section-{{ section.id }}-margins {% if section.settings.page_width == 'normal' %} page-width-desktop-max-width-only{% endif %}">
    <slideshow-component class="slider-mobile-gutter {% if section.settings.show_text_below %} mobile-text-below{% endif %} section-{{ section.id }}-padding" role="region" aria-roledescription="{{ 'sections.slideshow.carousel' | t }}" aria-label="{{ section.settings.accessibility_info | escape }}" >
        <div class="slideshow banner banner--{{ section.settings.slide_height }} grid grid--1-col slider slider--everywhere{% if section.settings.show_text_below %} banner--mobile-bottom{% endif %}{% if section.blocks.first.settings.image == blank %} slideshow--placeholder{% endif %}"
        id="Slider-{{ section.id }}"
        aria-live="polite"
        aria-atomic="true"
        data-autoplay="{{ section.settings.auto_rotate }}"
        data-speed="{{ section.settings.change_slides_speed }}"
        >
            {%- for block in section.blocks -%}
                {% style %}
                    :root:root  #Slide-{{ section.id }}-{{ forloop.index }} .banner__media::after  {
                        --color-base-background-1: {{ block.settings.overlay.red }}, {{ block.settings.overlay.green }}, {{ block.settings.overlay.blue }};
                        --gradient-base-background-1: {% if block.settings.overlay_gradient != blank %}{{ block.settings.overlay_gradient }}{% else %}{{ block.settings.overlay }}{% endif %};
                    }

                    #Slide-{{ section.id }}-{{ forloop.index }} .banner__media::after {
                        opacity: {{ block.settings.image_overlay_opacity | divided_by: 100.0 }};
                        background: var(--gradient-base-background-1);
                    }

                    @media screen and (max-width: 749px) {
                        .banner--mobile-bottom #Slide-{{ section.id }}-{{ forloop.index }} .banner__content {
                            --color-base-background-1: {{ block.settings.mob_below_overlay.red }}, {{ block.settings.mob_below_overlay.green }}, {{ block.settings.mob_below_overlay.blue }};
                            --gradient-base-background-1: {% if block.settings.mob_below_overlay_gradient != blank %}{{ block.settings.mob_below_overlay_gradient }}{% else %}{{ block.settings.mob_below_overlay }}{% endif %};
                            background: var(--gradient-base-background-1);
                            background-color: var(--gradient-base-background-1);
                        }

                        :root:root #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .slideshow__slide .banner__content .banner__box-{{ block.id }} {
                            --color-base-background-1: {{ block.settings.banner_background.red }}, {{ block.settings.banner_background.green }}, {{ block.settings.banner_background.blue }};
                            --gradient-base-background-1: {% if block.settings.banner_background_gradient != blank %}{{ block.settings.banner_background_gradient }}{% else %}{{ block.settings.banner_background }}{% endif %};
                            background: var(--gradient-base-background-1);
                            background-color: var(--gradient-base-background-1);
                        }
                    }

