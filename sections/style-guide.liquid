						render 'component-video', video: media, autoplay: true, poster_width: 800, aspect_ratio: 'natural'
					endfor

					assign video_url = 'surfer.mp4' | file_url
					render 'component-video', video: video_url, autoplay: true, controls: true

					render 'component-video', video: video_url, autoplay: true, controls: true, aspect_ratio: 'natural'

					assign video_url = 'Time_Lapse_Video_Of_Clouds.mp4' | file_url
					assign image = product.media | where: 'media_type', 'image' | first
					render 'component-video', video: video_url, autoplay: false, poster: image
				%}
			</div> {% endcomment %}
		{% comment %} </section> {% endcomment %}
	</div>
</div>
