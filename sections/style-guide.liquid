<div class="page-width" data-section-id="{{ section.id }}">
	<div class="page-header">
		<h1 class="uppercase">{{ page.title }}</h1>
	</div>

	<div>
        <style>
            :root:root {
                --color-base-background-1: {{ settings.colors_background_1.red }}, {{ settings.colors_background_1.green }}, {{ settings.colors_background_1.blue }};
            }

            .buttons-container {
				margin: 16px 0;
			}

			.buttons-container-arrows {
				height: 20rem;
				width: 20rem;
				position: relative;
				border: 2px solid rgb(var(--color-base-text));
			}

			.buttons-container .button {
				margin: 0 24px 16px 0;
			}

			.icons-grid {
				display: grid;
				grid-template-columns: repeat(auto-fill, minmax(100px, 1fr));
				grid-auto-rows: 60px;
				margin: 48px 0;
			}

			.icons-grid--lg .icon {
				width: 30px;
				height: 30px;
			}

			.slider-button {
				position: absolute;
			}

			.slider-button--up {
				top: 0;
				right: 8rem;
			}

			.slider-button--next {
				right: 0;
				bottom: 8rem;
			}

			.slider-button--down {
				bottom: 0;
				right: 8rem;
			}

			.slider-button--prev {
				bottom: 8rem;
				left: 0;
			}
        </style>
		{% comment %} <style>
			section {
				margin-bottom: 48px;
			}

			.section-title {
				margin-bottom: 16px;
			}

			.section-subtitle {
				margin-top: 32px;
			}

			.buttons-bg,
			.form-bg {
				margin-bottom: 32px;
				padding: 2px 32px;
				background: #000000;
				color: #ffffff;
			}

			.form-bg {
				margin-top: 48px;
				padding: 32px;
			}

			section .form__group input:not([type='checkbox']):not([type='radio']),
			section .form__group textarea {
				width: 100%;
			}

			.form__group--flex {
				display: flex;
				flex-wrap: wrap;
				align-items: center;
			}

			.form__group--flex input {
				margin-right: 10px;
			}

			.featured-blog__grid {
				list-style: none;
				padding: 0;
				margin: 0;
				display: grid;
				grid-template-columns: 1fr;
				gap: 24px;
			}

			@media only screen and (min-width: 61.875rem) {
				.featured-blog__grid {
					grid-template-columns: repeat(3, 1fr);
				}
			}

			.controls-demo {
				position: relative;
				height: 200px;
				margin-bottom: 16px;
				border: 1px solid #000000;
			}
		</style> {% endcomment %}

