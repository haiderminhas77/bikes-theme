{% schema %}
{
    "name": "t:sections.rich-text.name",
    "tag": "section",
    "class": "section",
    "disabled_on": {
        "groups": ["header", "footer"]
    },
    "settings": [
        {
            "type": "header",
            "content": "Section Width"
        },
        {
            "type": "select",
            "id": "page_width",
            "options": [
                {
                    "value": "normal",
                    "label": "normal"
                },
                {
                    "value": "full",
                    "label": "full"
                }
            ],
            "default": "full",
            "label": "Add a set page width to desktop"
        },
        {
            "type": "range",
            "id": "margin_left_right_desktop",
            "min": 0,
            "max": 40,
            "step": 1,
            "default": 0,
            "unit": "px",
            "label": "Margin left/right"
        },
        {
            "type": "range",
            "id": "margin_left_right_mob",
            "min": 0,
            "max": 40,
            "step": 1,
            "default": 0,
            "unit": "px",
            "label": "Margin left/right mob"
        },
        {
            "type": "header",
            "content": "Background Colour"
        },
        {
            "type": "checkbox",
            "id": "banner_transparent",
            "default": true,
            "label": "Enable banner transparency",
            "info": "put background opacity to 100%"
        },
        {
            "type": "color",
            "id": "colors_background_1",
            "default": "rgba(0,0,0,0)",
            "label": "Background"
        },
        {
            "id": "gradient_background_1",
            "type": "color_background",
            "label": "Background gradient"
        },
        {
            "type": "range",
            "id": "image_overlay_opacity",
            "min": 0,
            "max": 100,
            "step": 10,
            "unit": "%",
            "label": "Background opacity",
            "default": 100
        },
        {
            "type": "header",
            "content": "Banner"
        },
        {
            "type": "select",
            "id": "image_height",
            "options": [
                {
                    "value": "xsmall",
                    "label": "X small"
                },
                {
                    "value": "small",
                    "label": "t:sections.image-banner.settings.image_height.options__1.label"
                },
                {
                    "value": "medium",
                    "label": "t:sections.image-banner.settings.image_height.options__2.label"
                },
                {
                    "value": "large",
                    "label": "t:sections.image-banner.settings.image_height.options__3.label"
                },
                {
                    "value": "height-custom",
                    "label": "height custom"
                }
            ],	
            "default": "small",
            "label": "t:sections.image-banner.settings.image_height.label"
        },
        {
            "type": "range",
            "id": "height_custom",
            "min": 10,
            "max": 800,
            "step": 10,
            "unit": "px",
            "label": "Custom height on desktop",
            "default": 300
        },
        {
            "type": "range",
            "id": "height_custom_mob",
            "min": 0,
            "max": 500,
            "step": 10,
            "unit": "px",
            "label": "Custom height on mob",
            "default": 180
        },
        {
            "type": "url",
            "id": "link",
            "label": "Banner link"
        },
        {
            "type": "checkbox",
            "id": "link_new_tab",
            "default": false,
            "label": "Open link in new tab"
        },
        {
            "type": "header",
            "content": "Desktop layout"
        },
        {
            "type": "select",
            "id": "desktop_content_position",
            "options": [
                {
                    "value": "top-left",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__1.label"
                },
                {
                    "value": "top-center",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__2.label"
                },
                {
                    "value": "top-right",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__3.label"
                },
                {
                    "value": "middle-left",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__4.label"
                },
                {
                    "value": "middle-center",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__5.label"
                },
                {
                    "value": "middle-right",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__6.label"
                },
                {
                    "value": "bottom-left",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__7.label"
                },
                {
                    "value": "bottom-center",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__8.label"
                },
                {
                    "value": "bottom-right",
                    "label": "t:sections.image-banner.settings.desktop_content_position.options__9.label"
                }
            ],
            "default": "middle-center",
            "label": "t:sections.image-banner.settings.desktop_content_position.label",
