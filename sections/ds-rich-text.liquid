{% comment %}
    Renders 'ds-hero' in sections everywhere
	previously 'image-banner'
{% endcomment %}

{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}

{%- style -%}
    #shopify-section-{{ section.id }} .banner__content {
        padding: unset;
    }

	.section-{{ section.id }}-padding {
		margin-top: {{ section.settings.padding_top_mobile }}px;
		margin-bottom: {{ section.settings.padding_bottom_mobile }}px;
	}

	@media screen and (min-width: 750px) {
		.section-{{ section.id }}-padding {
			margin-top: {{ section.settings.padding_top }}px;
			margin-bottom: {{ section.settings.padding_bottom }}px;
		}
	}
    {% if section.settings.banner_transparent %}
        :root:root #Banner-{{ section.id }} {
            background: transparent;
        }
        :root:root #Banner-{{ section.id }}::after  {
            --color-base-background-1: unset;
            --gradient-base-background-1: unset;
        }

        #Banner-{{ section.id }}::after {
            opacity: {{ section.settings.image_overlay_opacity | divided_by: 100.0 }};
            background: var(--gradient-base-background-1);
            /* background-attachment: fixed; */
        }
    {% else %}
        :root:root #Banner-{{ section.id }}::after  {
            --color-base-background-1: {{ section.settings.colors_background_1.red }}, {{ section.settings.colors_background_1.green }}, {{ section.settings.colors_background_1.blue }};
            --gradient-base-background-1: {% if section.settings.gradient_background_1 != blank %}{{ section.settings.gradient_background_1 }}{% else %}{{ section.settings.colors_background_1 }}{% endif %};
        }

        #Banner-{{ section.id }}::after {
            opacity: {{ section.settings.image_overlay_opacity | divided_by: 100.0 }};
            background: var(--gradient-base-background-1);
            /* background-attachment: fixed; */
        }
    {% endif %}


    /* addded mob banner height for promo blocks */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner--height-custom:not(.banner--adapt) .banner__media {
            min-height: {{ section.settings.height_custom_mob | divided_by: 10.0 }}rem;
        }
    }
    /* addded desktop banner height for promo blocks */
    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner--height-custom:not(.banner--adapt) {
            min-height: {{ section.settings.height_custom | divided_by: 10.0 }}rem;
        }
    }


    /* addded mobile content positioning */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner--xsmall:not(.banner--adapt) {
            min-height: unset;
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            padding-top: {{ section.settings.banner__box_padding_top | divided_by: 10.0 }}rem;
            padding-bottom: {{ section.settings.banner__box_padding_bottom | divided_by: 10.0 }}rem;
            padding-left: {{ section.settings.banner__box_padding_left | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.banner__box_padding_right | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            padding-top: {{ section.settings.banner__box_padding_top_mob | divided_by: 10.0 }}rem;
            padding-bottom: {{ section.settings.banner__box_padding_bottom_mob | divided_by: 10.0 }}rem;
            padding-left: {{ section.settings.banner__box_padding_left_mob | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.banner__box_padding_right_mob | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            max-width: {{ section.settings.width_mob }}% - calc({{ section.settings.mobile_content_position_left  | divided_by: 10.0 }}rem + {{ section.settings.mobile_content_position_right  | divided_by: 10.0 }}rem));
            width: {{ section.settings.width_mob }}% - calc({{ section.settings.mobile_content_position_left  | divided_by: 10.0 }}rem + {{ section.settings.mobile_content_position_right  | divided_by: 10.0 }}rem));
            min-width: {{ section.settings.width_mob }}% - calc({{ section.settings.mobile_content_position_left  | divided_by: 10.0 }}rem + {{ section.settings.mobile_content_position_right  | divided_by: 10.0 }}rem));
        }
    }
    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content .banner__box {
            max-width: calc({{ section.settings.width }}% - calc({{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem + {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem));
            min-width: calc({{ section.settings.width }}% - calc({{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem + {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem));
            width: calc({{ section.settings.width }}% - calc({{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem + {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem));
        }
    }

    /* left */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-top-left .banner__box {
            left: {{ section.settings.mobile_content_position_left | divided_by: 10.0 }}rem;
            top: {{ section.settings.mobile_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-middle-left .banner__box  {
            left: {{ section.settings.mobile_content_position_left | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-bottom-left .banner__box  {
            bottom: {{ section.settings.mobile_content_position_bottom | divided_by: 10.0 }}rem;
            left: {{ section.settings.mobile_content_position_left | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--top-left .banner__box {
            left: {{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem;
            top: {{ section.settings.desktop_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--middle-left .banner__box  {
            left: {{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--bottom-left .banner__box  {
            bottom: {{ section.settings.desktop_content_position_bottom | divided_by: 10.0 }}rem;
            left: {{ section.settings.desktop_content_position_left | divided_by: 10.0 }}rem;
        }
    }

    /* right */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-top-right .banner__box {
            right: {{ section.settings.mobile_content_position_right | divided_by: 10.0 }}rem;
            top: {{ section.settings.mobile_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-middle-right .banner__box  {
            right: {{ section.settings.mobile_content_position_right | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-bottom-right .banner__box  {
            bottom: {{ section.settings.mobile_content_position_bottom | divided_by: 10.0 }}rem;
            right: {{ section.settings.mobile_content_position_right | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--top-right .banner__box {
            right: {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem;
            top: {{ section.settings.desktop_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--middle-right .banner__box {
            right: {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--bottom-right .banner__box {
            right: {{ section.settings.desktop_content_position_right | divided_by: 10.0 }}rem;
            bottom: {{ section.settings.desktop_content_position_bottom | divided_by: 10.0 }}rem;
        }
    }

    /* center */
    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-top-center .banner__box {
            top: {{ section.settings.mobile_content_position_top | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-middle-center .banner__box {
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--mobile-bottom-center .banner__box {
            bottom: {{ section.settings.mobile_content_position_bottom | divided_by: 10.0 }}rem;
        }

        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_mob | divided_by: 10.0 }}rem;
        }
    }

    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .banner__content.banner__content--bottom-center .banner__box {
            bottom: {{ section.settings.desktop_content_position_bottom | divided_by: 10.0 }}rem;
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--middle-center .banner__box {
        }

        #shopify-section-{{ section.id }} .banner__content.banner__content--top-center .banner__box {
            top: {{ section.settings.desktop_content_position_top | divided_by: 10.0 }}rem;
        }
        
        .section-{{ section.id }}-margins {
            padding-left: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
            padding-right: {{ section.settings.margin_left_right_desktop | divided_by: 10.0 }}rem;
        }
    }

    :root:root #Banner-{{ section.id }} .banner__box  {
		--color-base-background-1: {{ section.settings.banner_background.red }}, {{ section.settings.banner_background.green }}, {{ section.settings.banner_background.blue }};
		--gradient-base-background-1: {% if section.settings.banner_background_gradient != blank %}{{ section.settings.banner_background_gradient }}{% else %}{{ section.settings.banner_background }}{% endif %};
	}
{%- endstyle -%}

<div class="section-{{ section.id }}-margins {% if section.settings.page_width == 'normal' %} page-width-desktop-max-width-only{% endif %}">
	<div id="Banner-{{ section.id }}" class="banner banner--content-align-{{ section.settings.desktop_content_alignment }} banner--content-align-mobile-{{ section.settings.mobile_content_alignment }} banner--{{ section.settings.image_height }} banner--desktop-transparent {% if section.settings.full_width %} rich-text--full-width{% endif %} section-{{ section.id }}-padding">
        {% if section.settings.link != blank %}
            <a href="{{ section.settings.link }}" {% if section.settings.link_new_tab %}target="_blank"{% endif %}><div class="banner__full-link"></div></a>
        {% endif %}
        <div class="banner__media media">
        </div>
		<div class="banner__content banner__content--{{ section.settings.desktop_content_position }} banner__content--mobile-{{ section.settings.mobile_content_position }}">
			<div class="banner__box color-background-1 gradient">
			{%- for block in section.blocks -%}
				{%- case block.type -%}
					{%- when 'heading' -%}
						{% style %}
							@media screen and (max-width: 749px) {
								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__heading {
									color: {{ block.settings.heading_color }};
								}
							}
							@media screen and (min-width: 750px) {
								#shopify-section-{{ section.id }} .banner .banner__heading {
									color: {{ block.settings.heading_color }};
								}
							}
                            /* Desktop custom heading */
                            @media screen and (min-width: 1024px) {
                                #shopify-section-{{ section.id }} .banner__heading.custom-desktop {
                                    font-size: {{ block.settings.heading_size_custom | divided_by: 10.0 }}rem;
                                }
                            }
                            /* Mobile custom heading */
                            @media screen and (max-width: 1023px) {
                                #shopify-section-{{ section.id }} .banner__heading.custom-mobile {
                                    font-size: {{ block.settings.heading_size_mobile_custom | divided_by: 10.0 }}rem;
                                }
                            }

                            #shopify-section-{{ section.id }} .block-{{ block.id }}-padding {
                                margin-top: {{ block.settings.padding_top_mobile | divided_by: 10.0 }}rem;
                                margin-bottom: {{ block.settings.padding_bottom_mobile | divided_by: 10.0 }}rem;
                                padding-bottom: {{ block.settings.margin_top_mobile | divided_by: 10.0 }}rem;
                                padding-bottom: {{ block.settings.margin_bottom_mobile | divided_by: 10.0 }}rem;
                            }
                            @media screen and (min-width: 750px) {
                                #shopify-section-{{ section.id }} .block-{{ block.id }}-padding {
                                    margin-top: {{ block.settings.padding_top | divided_by: 10.0 }}rem;
                                    margin-bottom: {{ block.settings.padding_bottom | divided_by: 10.0 }}rem;
                                    padding-top: {{ block.settings.margin_top | divided_by: 10.0 }}rem;
                                    padding-bottom: {{ block.settings.margin_bottom | divided_by: 10.0 }}rem;
                                }
                            }

                            {% if block.settings.heading_font_family == 'heading-font' %}
                                #shopify-section-{{ section.id }} .banner__heading {
                                    font-family: var(--font-heading-family);
                                    font-weight: var(--font-heading-weight);
                                    line-height: 1;
                                }
                                {% else %}
                                #shopify-section-{{ section.id }} .banner__heading {
                                    font-family: var(--font-caption-family);
                                    font-weight: var(--font-caption-weight);
                                    letter-spacing: var(--font-caption-letter-spacing-wide);
                                    line-height: 1;
                                }
                            {% endif %}

                            #shopify-section-{{ section.id }} .banner__heading p {
                                margin: 0;
                                padding: 0;
                            }

                            /* @media screen and (max-width: 749px) {
                                .block-{{ block.id }}-width {
                                    width: {{ block.settings.width_mob | divided_by: 10.0 }}rem;
                                }
                            }
                            @media screen and (min-width: 750px) {
                                .block-{{ block.id }}-width {
                                    width: {{ block.settings.width | divided_by: 10.0  }}rem;
                                }
                            } */
						{% endstyle %}
						<h2 class="banner__heading {{ block.settings.heading_size }}-desktop {{ block.settings.heading_size_mobile }}-mobile  {% if block.settings.heading_uppercase %} uppercase {% endif %} block-{{ block.id }}-padding{% if block.settings.heading_center %} center{% endif %}" {{ block.shopify_attributes }}>
							<span>{{ block.settings.heading }}</span>
						</h2>
					{%- when 'text' -%}
						{% style %}
							@media screen and (max-width: 749px) {
								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__text {
									color:  {{ block.settings.text_color }};
								}

								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__text a {
									color:  {{ block.settings.text_link_color }};
								}
							}
							@media screen and (min-width: 750px) {
								#shopify-section-{{ section.id }} .banner .banner__text {
									color: {{ block.settings.text_color }};
								}

								#shopify-section-{{ section.id }} .banner .banner__text a {
									color: {{ block.settings.text_link_color }};
								}
							}

                            /* Desktop custom font size */
                            @media screen and (min-width: 1024px) {
                                #shopify-section-{{ section.id }} .banner__text.custom-desktop {
                                    font-size: {{ block.settings.text_size_custom | divided_by: 10.0 }}rem;
                                }
                            }
                            /* Mobile custom font size */
                            @media screen and (max-width: 1023px) {
                                #shopify-section-{{ section.id }} .banner__text.custom-mobile {
                                    font-size: {{ block.settings.text_size_mobile_custom | divided_by: 10.0 }}rem;
                                }
                            }

                            #shopify-section-{{ section.id }} .banner__text p {
                                margin: 0;
                                padding: 0;
                            }

                            #shopify-section-{{ section.id }} .block-{{ block.id }}-padding {
                                margin-top: {{ block.settings.padding_top_mobile | divided_by: 10.0 }}rem;
                                margin-bottom: {{ block.settings.padding_bottom_mobile | divided_by: 10.0 }}rem;
                            }
                            @media screen and (min-width: 750px) {
                                #shopify-section-{{ section.id }} .block-{{ block.id }}-padding {
                                    margin-top: {{ block.settings.padding_top | divided_by: 10.0 }}rem;
                                    margin-bottom: {{ block.settings.padding_bottom | divided_by: 10.0 }}rem;
                                }
                            }
                        
                            #shopify-section-{{ section.id }} .banner__text {
                                font-family:  var(--font-body-family);
                                line-height: 1.3;
                            }
						{% endstyle %}
						<div class="banner__text {{ block.settings.text_size }}-desktop {{ block.settings.text_size_mobile }}-mobile{% if block.settings.text_uppercase %} uppercase{% endif %}{% if block.settings.text_hide_mob %} small-hide{% endif %}{% if block.settings.text_hide_desk %} medium-hide large-up-hide{% endif %} block-{{ block.id }}-padding" {{ block.shopify_attributes }}>
							{{ block.settings.text }}
						</div>
					{%- when 'buttons' -%}
						{% style %}
                            @media screen and (max-width: 749px) {
                                /* mob primary 1 */
								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.button--primary {
									--color-button: {{ block.settings.button_color.red }}, {{ block.settings.button_color.green }}, {{ block.settings.button_color.blue }};
									--color-button-text: {{ block.settings.button_text_color.red }}, {{ block.settings.button_text_color.green }}, {{ block.settings.button_text_color.blue }};
								}

                                /* mob primary 1 [hover] */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.button--primary:not([disabled]):hover {
                                    --color-button: {{ block.settings.button_color_hover.red }}, {{ block.settings.button_color_hover.green }}, {{ block.settings.button_color_hover.blue }};
                                    --color-button-text: {{ block.settings.button_text_color_hover.red }}, {{ block.settings.button_text_color_hover.green }}, {{ block.settings.button_text_color_hover.blue }};
                                }

                                /* mob arrow 1 */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.link-with-icon {
									--color-link: {{ block.settings.button_arrow_color.red }}, {{ block.settings.button_arrow_color.green }}, {{ block.settings.button_arrow_color.blue }};
								}

                                /* mob arrow 1 [hover] */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.link-with-icon:not([disabled]):hover {
                                    --color-link: {{ block.settings.button_arrow_color_hover.red }}, {{ block.settings.button_arrow_color_hover.green }}, {{ block.settings.button_arrow_color_hover.blue }};
                                }

                                /* mob underline 1 */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.button--underline {
									--color-button-text: {{ block.settings.button_arrow_color.red }}, {{ block.settings.button_arrow_color.green }}, {{ block.settings.button_arrow_color.blue }};
								}

                                /* mob underline 1 [hover] */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.button--underline:not([disabled]):hover {
                                    --color-button-text: {{ block.settings.button_arrow_color_hover.red }}, {{ block.settings.button_arrow_color_hover.green }}, {{ block.settings.button_arrow_color_hover.blue }};
                                }

                                /* mob outline 1 */
								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.button--secondary {
									--color-button: {{ block.settings.button_outline_color.red }}, {{ block.settings.button_outline_color.green }}, {{ block.settings.button_outline_color.blue }};
									--color-button-text: {{ block.settings.button_outline_text_color.red }}, {{ block.settings.button_outline_text_color.green }}, {{ block.settings.button_outline_text_color.blue }};
								}

                                /* mob outline 1 [hover] */
                                    #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:first-of-type.button--secondary:not([disabled]):hover {
                                    --color-button: {{ block.settings.button_outline_color_hover.red }}, {{ block.settings.button_outline_color_hover.green }}, {{ block.settings.button_outline_color_hover.blue }};
                                    --color-button-text: {{ block.settings.button_outline_text_color_hover.red }}, {{ block.settings.button_outline_text_color_hover.green }}, {{ block.settings.button_outline_text_color_hover.blue }};
                                    --alpha-button-background: 1;
                                }
                                
                                /* mob primary 2 */
								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:nth-of-type(2).button--primary {
									--color-button: {{ block.settings.button_color_2.red }}, {{ block.settings.button_color_2.green }}, {{ block.settings.button_color_2.blue }};
									--color-button-text: {{ block.settings.button_text_color_2.red }}, {{ block.settings.button_text_color_2.green }}, {{ block.settings.button_text_color_2.blue }};
								}

                                /* mob primary 2 [hover] */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:nth-of-type(2).button--primary:not([disabled]):hover {
									--color-button: {{ block.settings.button_color_2_hover.red }}, {{ block.settings.button_color_2_hover.green }}, {{ block.settings.button_color_2_hover.blue }};
									--color-button-text: {{ block.settings.button_text_color_2_hover.red }}, {{ block.settings.button_text_color_2_hover.green }}, {{ block.settings.button_text_color_2_hover.blue }};
								}

                                /* mob arrow 2 */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:nth-of-type(2).link-with-icon {
									--color-link: {{ block.settings.button_arrow_color_2.red }}, {{ block.settings.button_arrow_color_2.green }}, {{ block.settings.button_arrow_color_2.blue }};
								}

                                /* mob arrow 2 [hover] */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:nth-of-type(2).link-with-icon:not([disabled]):hover {
                                    --color-link: {{ block.settings.button_arrow_color_hover_2.red }}, {{ block.settings.button_arrow_color_hover_2.green }}, {{ block.settings.button_arrow_color_hover_2.blue }};
                                }

                                /* mob underline 2 */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:nth-of-type(2).button--underline {
									--color-button-text: {{ block.settings.button_arrow_color_2.red }}, {{ block.settings.button_arrow_color_2.green }}, {{ block.settings.button_arrow_color_2.blue }};
								}

                                /* mob underline 2 [hover] */
                                #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:nth-of-type(2).button--underline:not([disabled]):hover {
                                    --color-button-text: {{ block.settings.button_arrow_color_hover_2.red }}, {{ block.settings.button_arrow_color_hover_2.green }}, {{ block.settings.button_arrow_color_hover_2.blue }};
                                }
                                
                                /* mob outline 2 */
								#shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .banner__buttons a:nth-of-type(2).button--secondary {
