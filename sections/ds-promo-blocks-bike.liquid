{% comment %}
	Renders 'ds-promo-blocks' in sections everywhere
{% endcomment %}

{{ 'section-multicolumn.css' | asset_url | stylesheet_tag }}
{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}
{{ 'template-collection.css' | asset_url | stylesheet_tag }}

{%- liquid
	assign blocks_to_display = section.blocks.size

	assign show_slider_on_mobile = false
	if section.settings.enable_mobile_slider and blocks_to_display > 2
		assign show_slider_on_mobile = true
	endif

	assign show_slider_on_desktop = false
	if section.settings.enable_desktop_slider and blocks_to_display > 2
		assign show_slider_on_desktop = true
	endif

    assign show_title_wrapper_on_desktop = false
    if section.settings.title != blank and show_slider_on_desktop
        assign show_title_wrapper_on_desktop = true
    elsif section.settings.title != blank and show_slider_on_desktop == false
        assign show_title_wrapper_on_desktop = true
    elsif section.settings.title == blank and show_slider_on_desktop == false
        assign show_title_wrapper_on_desktop = false
    elsif section.settings.title == blank and show_slider_on_desktop
        assign show_title_wrapper_on_desktop = true
	endif

    assign show_title_wrapper_on_mob = false
    if section.settings.title != blank
        assign show_title_wrapper_on_mob = true
	endif
-%}

{%- if section.settings.image_ratio != 'custom' %}
    {%- liquid
        assign highest_ratio = 0
        for block in section.blocks
        if block.settings.image.aspect_ratio > highest_ratio
            assign highest_ratio = block.settings.image.aspect_ratio
            endif
        endfor 
    -%}
    
    {%- liquid
        assign highest_ratio_mob = 0
        for block in section.blocks
        if block.settings.image_mob.aspect_ratio > highest_ratio_mob
            assign highest_ratio_mob = block.settings.image_mob.aspect_ratio
            endif
        endfor 
    -%}
    {% style %}
        @media screen and (max-width: 749px) {
			#Banner-{{ section.id }} .banner__media::before,
			#Banner-{{ section.id }}:not(.banner--mobile-bottom) .banner__content::before {
                {% if section.settings.image_ratio == 'adapt' and section.blocks.first.settings.image_mob %}
                    padding-bottom: {{ 1 | divided_by: highest_ratio_mob | times: 100 }}%;
                {% elsif section.settings.image_ratio == 'adapt' %}
                    padding-bottom: {{ 1 | divided_by: highest_ratio | times: 100 }}%;
                {% elsif section.settings.image_ratio == 'square' %}
                    padding-bottom: 100%;
                {% elsif section.settings.image_ratio == 'portrait' %}
                    padding-bottom: 125%;
                {% endif %}
                content: '';
                display: block;
            }
        }

        @media screen and (min-width: 750px) {
			#Banner-{{ section.id }} .banner__media::before {
                {% if section.settings.image_ratio == 'adapt' %}
                    padding-bottom: {{ 1 | divided_by: highest_ratio | times: 100 }}%;
                {% elsif section.settings.image_ratio == 'square' %}
                    padding-bottom: 100%;
                {% elsif section.settings.image_ratio == 'portrait' %}
                    padding-bottom: 125%;
                {% endif %}
                content: '';
                display: block;
            }
        }
    {% endstyle %}
{% endif %}

