        }
    }

    {% if section.settings.hide_bar != true %}
        /* repositioned to top right on desktop, below center on mobile */
        @media screen and (max-width: 749px) {
            #shopify-section-{{ section.id }} .progress-slider .progress-slider__wrapper {
                margin-left: {{ section.settings.margin_left_mob | divided_by: 10.0 }}rem;
            }
        }

        /* repositioned to top right on desktop, below center on mobile */
        @media screen and (min-width: 749px) {
            #shopify-section-{{ section.id }} .progress-slider .progress-slider__wrapper {
                margin-right: {{ section.settings.margin_left_desktop | divided_by: 10.0 }}rem;
                margin-left: {{ section.settings.margin_left_desktop | divided_by: 10.0 }}rem;
            }
        }
    {% endif %}

    /* progress slider colour settings */
    #shopify-section-{{ section.id }} .progress-slider .slider-button {
        --color-base-background-2: {{ section.settings.slider_arrows_background.red }},  {{ section.settings.slider_arrows_background.green }}, {{ section.settings.slider_arrows_background.blue }};
        --color-custom-background: {{ section.settings.slider_arrows.red }},  {{ section.settings.slider_arrows.green }}, {{ section.settings.slider_arrows.blue }};
    }

    #shopify-section-{{ section.id }} .progress-slider .slider-counter--lines {
        --color-foreground: {{ section.settings.slider_progress_bar.red }},  {{ section.settings.slider_progress_bar.green }}, {{ section.settings.slider_progress_bar.blue }};
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .title {
            font-size: {{ section.settings.title_size_mobile_custom | divided_by: 10.0 }}rem;
        }
    }
    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .title {
            font-size: {{ section.settings.title_size_custom | divided_by: 10.0 }}rem;
        }
    }

    #shopify-section-{{ section.id }} .title {
        font-family: var(--font-heading-family);
        letter-spacing: var(--font-heading-letter-spacing-narrow);
        line-height: 0.9;
    }

    @media screen and (max-width: 749px) {
        #shopify-section-{{ section.id }} .title_caption {
            font-size: {{ section.settings.title_caption_size_mobile_custom | divided_by: 10.0 }}rem;
        }
    }
    @media screen and (min-width: 750px) {
        #shopify-section-{{ section.id }} .title_caption {
            font-size: {{ section.settings.title_caption_size_custom | divided_by: 10.0 }}rem;
        }
    }

    #shopify-section-{{ section.id }} .title-wrapper-with-link .title_caption {
        color: {{ section.settings.title_caption_color }};
    }

    #shopify-section-{{ section.id }} .title_caption {
        font-family: var(--font-caption-family);
        line-height: 1.1;
        font-style: var(--font-caption-style);
        font-weight: var(--font-caption-weight);
    }


    {% if section.settings.text_first_desktop and section.settings.show_text_below_desktop %}
        @media screen and (min-width: 750px) {
            #shopify-section-{{ section.id }} .banner {
                    flex-direction: row;
                    flex-wrap: wrap-reverse;
                }
            }
    {% endif %}
    {% if section.settings.text_first_mob and section.settings.show_text_below %}
        @media screen and (max-width: 750px) {
            #shopify-section-{{ section.id }} .banner {
                    flex-direction: row;
                    flex-wrap: wrap-reverse;
                }
            }
    {% endif %}

    /* grid variables for promo grid gaps  */
    :root:root #shopify-section-{{ section.id }} .product-grid.grid {
        --grid-mobile-horizontal-spacing: {{ section.settings.spacing_grid_horizontal_mob | divided_by: 10.0 }}rem;
        --grid-desktop-horizontal-spacing: {{ section.settings.spacing_grid_horizontal | divided_by: 10.0 }}rem;
        --grid-mobile-vertical-spacing: {{ section.settings.spacing_grid_vertical_mob | divided_by: 10.0 }}rem;
        --grid-desktop-vertical-spacing: {{ section.settings.spacing_grid_vertical | divided_by: 10.0 }}rem;
        margin-top: 0;
        margin-bottom: 0;
        padding-top: {{ section.settings.blocks_padding_top | divided_by: 10.0 }}rem;
        padding-bottom: {{ section.settings.blocks_padding_bottom | divided_by: 10.0 }}rem;
    }

    @media screen and (max-width: 749px) {
        :root:root #shopify-section-{{ section.id }} .product-grid.grid {
            padding-top: {{ section.settings.blocks_padding_top_mobile | divided_by: 10.0 }}rem;
            padding-bottom: {{ section.settings.blocks_padding_bottom_mobile | divided_by: 10.0 }}rem;
        }
    }

    /* bike pdp - border radius on blocks */
    {% if section.settings.enable_border_radius %}
        #shopify-section-{{ section.id }} .multicolumn-card, #shopify-section-{{ section.id }} .multicolumn-card__image-wrapper {
            border-radius: 21px;
        }

        @media screen and (max-width: 749px) {
            {% if section.settings.enable_space_between_mob %}
                #shopify-section-{{ section.id }} .banner--desktop-bottom .banner__media {
                    border-bottom-right-radius: 21px;
                    border-bottom-left-radius: 21px;
                }
            {% endif %}
        }

        @media screen and (min-width: 750px) {
            {% if section.settings.enable_space_between %}
                #shopify-section-{{ section.id }} .banner--desktop-bottom .banner__media {
                    border-bottom-right-radius: 21px;
                    border-bottom-left-radius: 21px;
                }
            {% endif %}
        }
    {% endif %}

    /* bike pdp - box shadow on blocks - currently not working as expected with slider margin */
    {% if section.settings.enable_box_shadow %}
        #shopify-section-{{ section.id }} .multicolumn-card {
            box-shadow: -8px -8px 30px rgba(255, 255, 255, 0.25), 8px 8px 20px rgba(0, 0, 0, 0.2), inset 1px 2px 4px -1px rgba(255, 255, 255, 0.2);
        }
    {% endif %}

    /* bike pdp - spaces out block content to avoid empty space beneath images when blocks are different heights */
    {% if section.settings.enable_space_between %}
        @media screen and (min-width: 750px) {
            #shopify-section-{{ section.id }} .multicolumn-card__image-wrapper {
                min-height: 100%;
                justify-content: space-between;
            }
        }
    {% endif %}

    {% if section.settings.enable_space_between_mob %}
        @media screen and (max-width: 749px) {
            #shopify-section-{{ section.id }} .multicolumn-card__image-wrapper {
                min-height: 100%;
                justify-content: space-between;
            }
        }
    {% endif %}
{% endstyle %}

<div class="collection collection--full-width section-{{ section.id }}-padding {% if section.settings.page_width == 'normal' %} page-width-desktop-max-width-only{% endif %}{% if section.settings.hide_desktop %} medium-hide large-up-hide{% endif %}{% if section.settings.hide_mobile %} small-hide{% endif %}">
	<slider-component class="slider-mobile-gutter">
        {% unless section.settings.title or section.settings.title_caption  %}
            <div class="title-wrapper title-wrapper-with-link title-wrapper--self-padded-tablet-down {% if section.settings.title_center %} title-wrapper-center{% endif %}{% if show_title_wrapper_on_mob != true %} small-hide {% endif %}{% if show_title_wrapper_on_desktop != true %} medium-hide large-up-hide {% endif %}">
                {%- if section.settings.title_caption -%}
                    <p class="title_caption{% if section.settings.title_caption_uppercase %} uppercase{% endif %}">{{ section.settings.title_caption | escape }}</p>
                {%- endif -%}
                {% if section.settings.title != blank %}
                <h2 class="title{% if section.settings.title_uppercase %} uppercase{% endif %}">{{ section.settings.title | escape }}</h2>
                {% endif %}
            </div>
        {% endunless %}

		<ul id="Slider-{{ section.id }}" class="grid product-grid grid--{{ section.settings.columns_mobile }}-col-tablet-down grid--{{ section.settings.columns_desktop }}-col-desktop {% if show_slider_on_mobile or show_slider_on_desktop %} slider{%endif%}{% if show_slider_on_desktop %} slider--desktop{% endif %}{% if show_slider_on_mobile %} slider--tablet {% unless section.settings.enable_grid_peek != true %} grid--peek{% endunless %}{% endif %}{% if section.settings.flex_center %} flex-center-desktop{% endif %}" role="list" aria-label="{{ 'sections.featured_collection.slider' | t }}">
			{%- for block in section.blocks -%}
				<li id="Slide-{{ section.id }}-{{ forloop.index }}" class="grid__item{% if show_slider_on_mobile or show_slider_on_desktop %} slider__slide{% endif %}"  {{ block.shopify_attributes }}>
                    <div class="multicolumn-card color-background-custom gradient">
                        {% if block.settings.link != blank %}
                            <a href="{{ block.settings.link }}" {% if block.settings.link_new_tab %}target="_blank"{% endif %}><div class="banner__full-link{% if block.settings.image_zoom %} banner__full-link-hover{% endif %}"></div></a>
                        {% endif %}
						<div id="Banner-{{ section.id }}" class="banner banner--{{ section.settings.image_height }}{% if section.settings.show_text_below %} banner--mobile-bottom{% endif %}{% if section.settings.show_text_below_desktop %} banner--desktop-bottom{% endif %}  multicolumn-card__image-wrapper multicolumn-card__image-wrapper--full-width{% if section.settings.add_padding %} multicolumn-card-spacing{% endif %} banner--content-align-{{ block.settings.desktop_content_alignment }} banner--content-align-mobile-{{ block.settings.mobile_content_alignment }} banner--{{ section.settings.image_height }}{% if section.settings.image_ratio != 'custom' %} banner--adapt{% endif %} banner--desktop-transparent">
							{%- if block.settings.image != blank -%}
								<div class="banner__media media media--transparent media--adapt {% if block.settings.image == blank %} placeholder{% endif %}">
									{%- if block.settings.image_mob != blank -%}
                                        <picture>
                                            <source srcset="{%- if block.settings.image.width >= 275 -%}{{ block.settings.image | image_url: width: 275 }} 275w,{%- endif -%}
                                            {%- if block.settings.image.width >= 550 -%}{{ block.settings.image | image_url: width: 550 }} 550w,{%- endif -%}
                                            {%- if block.settings.image.width >= 710 -%}{{ block.settings.image | image_url: width: 710  }} 710w,{%- endif -%}
                                            {%- if block.settings.image.width >= 1420 -%}{{ block.settings.image | image_url: width: 1420 }} 1420w,{%- endif -%}
                                            {{ block.settings.image | image_url }} {{ block.settings.image.width }}w"
                                            media="(min-width: 750px)"
                                            sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                                            (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %},
                                            calc(100vw - 30px)"
                                            height="{{ block.settings.image.height }}"
                                            width="{{ block.settings.image.width }}"
                                            />
                                            <source srcset="{%- if block.settings.image_mob.width >= 275 -%}{{ block.settings.image_mob | image_url: width: 275 }} 275w,{%- endif -%}
                                            {%- if block.settings.image_mob.width >= 550 -%}{{ block.settings.image_mob | image_url: width: 550  }} 550w,{%- endif -%}
                                            {%- if block.settings.image_mob.width >= 710 -%}{{ block.settings.image_mob | image_url: width: 710  }} 710w,{%- endif -%}
                                            {%- if block.settings.image_mob.width >= 1420 -%}{{ block.settings.image_mob | image_url: width: 1420 }} 1420w,{%- endif -%}
                                            {{ block.settings.image_mob | image_url }} {{ block.settings.image_mob.width }}w"
                                            media="(max-width: 749px)"
                                            height="{{ block.settings.image_mob.height }}"
                                            width="{{ block.settings.image_mob.width }}"
                                            sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                                            (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %},
                                            calc(100vw - 30px)"
                                            />
                                            {{ block.settings.image | image_url: width: 710 | image_tag:
                                            loading: 'lazy',
                                            height: block.settings.image.height,
                                            width: block.settings.image.width,
                                            widths: '275, 550, 710'}}
                                        </picture>
                                    {%- else -%}
                                        {% capture image_class %}multicolumn-card__image {% if section.settings.image_height == 'height-custom' and section.settings.image_ratio == 'custom' %} custom-height {% endif %}{% endcapture %}
                                        {% if section.settings.columns_desktop >= 4 %}
                                            {%- capture block_image_height -%}{{ 710 | divided_by: block.settings.image.aspect_ratio | round }}{%- endcapture -%}
                                            {{ block.settings.image | image_url: width: 710 | image_tag:
                                            loading: 'lazy',
                                            sizes: '(min-width 750px) 710px, 710px',
                                            widths: '375, 710',
                                            width: 710,
                                            height: block_image_height,
                                            class: image_class }}   
                                        {% else %}
                                        {%- capture block_image_height -%}{{ 710 | divided_by: block.settings.image.aspect_ratio | round }}{%- endcapture -%}
                                            {{ block.settings.image | image_url: width: 710 | image_tag:
                                            loading: 'lazy',
                                            widths: '375, 550, 710',
                                            width: block.settings.image.width,
                                            height: block_image_height,
                                            class: image_class }}
                                        {% endif %}
                                    {%- endif -%}
								</div>
							{%- else -%}
								<div class="banner__media media media--transparent placeholder">
                                    {%- if block.settings.show_placeholder -%}
                                        {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
                                    {%- endif -%}
								</div>
							{%- endif -%}
							<div class="banner__content banner__content-{{ block.id }} banner__content--{{ block.settings.desktop_content_position }} banner__content--mobile-{{ block.settings.mobile_content_position }}">
								<div class="banner__box banner__box-{{ block.id }} color-background-custom gradient">
                                    {%- if block.settings.header_image != blank or block.settings.header_svg != blank-%}	
                                        <div class="banner__image block-{{ block.id }}-padding" {{ block.shopify_attributes }} {% if block.settings.banner_image_center %} style="text-align: center; margin-right: auto; margin-left: auto;"{% endif %}>
                                            <div class="media media--transparent" style="padding-top: {{ 1 | divided_by: block.settings.header_image.aspect_ratio | times: 100 }}%">
                                                {%- if block.settings.header_image != blank -%}	
                                                    {% assign header_image_width = block.settings.header_image_width %}
                    
                                                    {% assign header_image_width_string = block.settings.header_image_width | append: ''%}
                                                    {{ block.settings.header_image | image_url: width: header_image_width | image_tag:	
                                                    loading: 'lazy',	
                                                    height: block.settings.header_image.height,
                                                    width: block.settings.header_image.width,
                                                    widths: header_image_width_string
                                                    }}	
                                                {%- elsif block.settings.header_svg != blank -%}	
                                                    {{ block.settings.header_svg }}	
                                                {%- endif -%}
                                            </div>
                                        </div>
                                        {% style %}
                                            .block-{{ block.id }}-padding {
                                                margin-top: {{ block.settings.padding_top_mobile | divided_by: 10.0 }}rem;
                                                margin-bottom: {{ block.settings.padding_bottom_mobile | divided_by: 10.0 }}rem;
                                            }
                
                                            @media screen and (min-width: 750px) {
                                                .block-{{ block.id }}-padding {
                                                    margin-top: {{ block.settings.padding_top | divided_by: 10.0 }}rem;
                                                    margin-bottom: {{ block.settings.padding_bottom | divided_by: 10.0 }}rem;
                                                }
                                            }

                                            @media screen and (max-width: 749px) {
                                                #shopify-section-{{ section.id }} .banner__box-{{ block.id }} .banner__image {
                                                    width: {{ block.settings.header_image_width_mob }}px;
                                                }
                                            }
                
                                            @media screen and (min-width: 750px) {
                                                #shopify-section-{{ section.id }} .banner__box-{{ block.id }} .banner__image {
                                                    width: {{ block.settings.header_image_width }}px;
                                                }
                                            }
                                            #shopify-section-{{ section.id }} .banner__box-{{ block.id }} .banner__image {
                                                color: {{ block.settings.svg_color }};
                                            }
                                        {% endstyle %}
                                    {%- endif -%}
                                {% if block.settings.caption != blank %}
                                    <div class="banner__caption banner__caption-{{ block.id }} {{ block.settings.caption_size }}-desktop {{ block.settings.caption_size_mobile }}-mobile{% if block.settings.caption_uppercase %} uppercase{% endif %} banner_caption-{{ block.id }}-padding">{{ block.settings.caption }}</div>
                                {% endif %}
                                    {% if block.settings.heading != blank %}
                                        <h3 class="banner__heading banner__heading-{{ block.id }} {{ block.settings.heading_size }}-desktop {{ block.settings.heading_size_mobile }}-mobile {% if block.settings.heading_uppercase %}uppercase {% endif %} banner_heading-{{ block.id }}-padding">
                                            <span>{{ block.settings.heading | escape }}</span>
                                        </h3>
                                    {% endif %}
									{%- if block.settings.text != blank -%}
										<div class="banner__text banner__text-{{ block.id }} {{ block.settings.text_size }}-desktop {{ block.settings.text_size_mobile }}-mobile{% if block.settings.text_uppercase %} uppercase{% endif %} banner__text-{{ block.id }}-padding">
											{{ block.settings.text }}
										</div>
									{%- endif -%}
                                    {%- if block.settings.button_label_1 != blank -%}
                                        <div class="banner__buttons banner__buttons-{{ block.id }} banner_buttons-{{ block.id }}-padding block-{{ block.id }}-padding" {{ block.shopify_attributes }}>
                                            <a{% if block.settings.button_link_1 == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link_1 }}"{% endif %} {% if block.settings.button_new_tab_1 %}target="_blank"{% endif %} class="{% if block.settings.button_style_1 != 'arrow' %}button{% endif %}{% if block.settings.button_style_1 == 'arrow' %} link link-with-icon{% endif %}{% if block.settings.button_style_1 == 'outline' %} button--secondary{% elsif block.settings.button_style_1 == 'primary' %} button--primary{% elsif block.settings.button_style_1 == 'underline' %} button--underline{% endif %}{% if block.settings.button_uppercase_1 %} uppercase{% endif %} {% if block.settings.button_wide_1 %} button--wide{% endif %}">
                                                {{ block.settings.button_label_1 | escape }}{% if block.settings.button_style_1 == 'arrow'%}{%- render 'icon-arrow-right' -%}{% endif %}
                                            </a>
                                        </div>
                                    {%- endif -%}
                                    {% if block.settings.link != blank %}
                                        <a href="{{ block.settings.link }}" {% if block.settings.link_new_tab %}target="_blank"{% endif %}><div class="banner__full-link"></div></a>
                                    {% endif %}
								</div>
							</div>
						</div>
					</div>
				</li>
			{% endfor %}
		</ul>
        {%- if blocks_to_display > 2 -%}
            {% comment %} <div class="slider-buttons no-js-hidden{% if show_slider_on_mobile == false %} small-hide {% endif %}{%if show_slider_on_desktop == false %} medium-hide large-up-hide{% endif %}{% if show_slider_on_mobile %} title-wrapper--self-padded-tablet-down{% endif %}">
                <button type="button" class="slider-button slider-button--prev" name="previous" aria-label="{{ 'general.slider.previous_slide' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-caret' %}</button>
                <div class="slider-counter caption">
                    <span class="slider-counter--current">1</span>
                    <span aria-hidden="true"> / </span>
                    <span class="visually-hidden">{{ 'general.slider.of' | t }}</span>
                    <span class="slider-counter--total">{{ blocks_to_display }}</span>
                </div>
                <button type="button" class="slider-button slider-button--next" name="next" aria-label="{{ 'general.slider.next_slide' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-caret' %}</button>
            </div> {% endcomment %}
            <div class="{% if show_slider_on_mobile == false %} small-hide{% endif %}{% if show_slider_on_desktop == false %} medium-hide large-up-hide{% endif %}">
                {% render 'progress-slider-bar', item_count: blocks_to_display, hide_bar: section.settings.hide_bar, slider_step_count: section.settings.slider_step_count %}
            </div>
		{%- endif -%}
	</slider-component>
</div>

{% schema %}
{
    "name": "DS Promo blocks",
    "tag": "section",
    "class": "section color-background-custom gradient",
    "disabled_on": {
        "groups": ["header", "footer"]
    },
    "settings": [
        {
            "type": "checkbox",
            "id": "hide_desktop",
            "default": false,
            "label": "Hide on desktop"
        },
        {
            "type": "checkbox",
            "id": "hide_mobile",
            "default": false,
            "label": "Hide on mobile"
        },
        {
            "type": "select",
            "id": "page_width",
            "options": [
                {
                    "value": "normal",
                    "label": "normal"
                },
                {
                    "value": "full",
                    "label": "full"
                }
            ],
            "default": "full",
            "label": "Add a set page width to desktop"
        },
        {
            "type": "range",
            "id": "margin_left_desktop",
            "min": 0,
            "max": 40,
            "step": 1,
            "default": 26,
            "unit": "px",
            "label": "Margin left/right"
        },
        {
            "type": "range",
            "id": "margin_left_mob",
            "min": 0,
            "max": 40,
            "step": 1,
            "default": 20,
            "unit": "px",
            "label": "Margin left/right mob"
        },
        {
            "type": "header",
            "content": "Background Colour"
        },
        {
            "type": "color",
            "id": "background_color",
            "default": "rgba(0,0,0,0)",
            "label": "Background"
        },
        {
            "id": "background_gradient",
            "type": "color_background",
            "label": "Background gradient"
        },
        {
            "type": "color",
            "id": "border_bottom",
            "default": "#c9c9c9",
            "label": "Border",
            "info": "Sits below title section on desktop, and above slider arrows on mob"
        },
        {
            "type": "checkbox",
            "id": "enable_border_radius",
            "default": false,
            "label": "Enable border radius on blocks"
        },
        {
            "type": "checkbox",
            "id": "enable_box_shadow",
            "default": false,
            "label": "Enable box shadow on blocks"
        },
        {
            "type": "header",
            "content": "Slider colour"
        },
        {
            "type": "color",
            "id": "slider_arrows",
            "default": "#ffffff",
            "label": "Arrows"
        },
        {
            "type": "color",
            "id": "slider_arrows_background",
            "default": "#4d4d4d",
            "label": "Arrows background"
        },
        {
            "type": "color",
            "id": "slider_progress_bar",
            "default": "#1c1c1c",
            "label": "Progress bar"
        },
        {
            "type": "checkbox",
            "id": "hide_bar",
            "label": "Hide progress bar",
            "default": true
        },
        {
            "type": "select",
            "id": "slider_positioning",
            "options": [
                {
                    "value": "above",
                    "label": "Above blocks"
                },
                {
                    "value": "below",
                    "label": "Below blocks"
                }
            ],
            "default": "above",
            "label": "Slider positioning [desktop only]"
        },
        {
            "type": "select",
            "id": "slider_alignment",
            "options": [
                {
                    "value": "left",
                    "label": "Left"
                },
                {
                    "value": "center",
                    "label": "Center"
                },
                {
                    "value": "right",
                    "label": "Right"
                },
                {
                    "value": "space-between",
                    "label": "Space Between"
                }
            ],
            "default": "right",
            "label": "Slider alignment [desktop]"
        },
        {
            "type": "select",
            "id": "slider_alignment_mob",
            "options": [
                {
                    "value": "left",
                    "label": "Left"
                },
                {
                    "value": "center",
                    "label": "Center"
                },
                {
                    "value": "right",
                    "label": "Right"
                },
                {
                    "value": "space-between",
                    "label": "Space Between"
                }
            ],
            "default": "center",
            "label": "Slider alignment [mob]"
        },
        {
            "type": "header",
            "content": "Title",
            "info": "Above promo blocks"
        },
        {
            "type": "text",
            "id": "title_caption",
            "label": "Caption",
            "info": "Will only show if title is added below"
        },
        {
            "type": "checkbox",
