                --color-base-background-1: {{ block.settings.overlay.red }}, {{ block.settings.overlay.green }}, {{ block.settings.overlay.blue }};
                --gradient-base-background-1: {% if block.settings.overlay_gradient != blank %}{{ block.settings.overlay_gradient }}{% else %}{{ block.settings.overlay }}{% endif %};
            }

            #Slide-{{ section.id }}-{{ forloop.index }} .banner__media::after {
                opacity: {{ block.settings.image_overlay_opacity | divided_by: 100.0 }};
                background: var(--gradient-base-background-1);
            }

            @media screen and (max-width: 749px) {
                .banner--mobile-bottom #Slide-{{ section.id }}-{{ forloop.index }} .banner__content {
                    --color-base-background-1: {{ block.settings.mob_below_overlay.red }}, {{ block.settings.mob_below_overlay.green }}, {{ block.settings.mob_below_overlay.blue }};
                    --gradient-base-background-1: {% if block.settings.mob_below_overlay_gradient != blank %}{{ block.settings.mob_below_overlay_gradient }}{% else %}{{ block.settings.mob_below_overlay }}{% endif %};
                    background: var(--gradient-base-background-1);
                    background-color: var(--gradient-base-background-1);
                }

                :root:root #shopify-section-{{ section.id }} .banner:not(.banner--mobile-bottom) .slideshow__slide .banner__content .banner__box-{{ block.id }} {
                    --color-base-background-1: {{ block.settings.banner_background.red }}, {{ block.settings.banner_background.green }}, {{ block.settings.banner_background.blue }};
                    --gradient-base-background-1: {% if block.settings.banner_background_gradient != blank %}{{ block.settings.banner_background_gradient }}{% else %}{{ block.settings.banner_background }}{% endif %};
                    background: var(--gradient-base-background-1);
                    background-color: var(--gradient-base-background-1);
