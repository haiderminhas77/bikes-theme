            "heading_background_color_active": "#D9D9D9",
            "heading_size_custom": 16,
            "heading_size_mobile_custom": 18,
            "icon": "none",
            "row_content": "<p>At the moment, we do not offer gift cards for purchase. We suggest signing up for our email newsletter for the latest updates.<\/p>",
            "custom_liquid": "",
