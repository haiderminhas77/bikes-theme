        "collapsible_row_RrDP43": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Can I split my payment between two different credit and\/or debit cards?",
            "heading_uppercase": true,
            "heading_color": "#000000",
            "heading_background_color": "#D9D9D9",
            "heading_color_active": "#000000",
            "heading_background_color_active": "#D9D9D9",
            "heading_size_custom": 16,
            "heading_size_mobile_custom": 16,
            "icon": "none",
            "row_content": "<p>At the moment, we are unable to offer this option. For monthly instalment payments, we offer <a href=\"https:\/\/eu.super73.com\/pages\/financing-payments\" target=\"_blank\" title=\"https:\/\/eu.super73.com\/pages\/financing-payments\"><strong>Splitit<\/strong><\/a><strong>.<\/strong><\/p>",
