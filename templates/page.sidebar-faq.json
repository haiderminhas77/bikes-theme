    "ds_collapsible_content_mcPGQc": {
      "type": "ds-collapsible-content",
      "blocks": {
        "collapsible_row_WCBAmy": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Do you offer financing?",
            "heading_uppercase": true,
            "heading_color": "#1c1c1c",
            "heading_background_color": "#D9D9D9",
            "heading_color_active": "#000000",
            "heading_background_color_active": "#D9D9D9",
            "heading_size_custom": 16,
            "heading_size_mobile_custom": 16,
            "icon": "none",
            "row_content": "<p>We currently offer installments via <strong>Klarna<\/strong> though specific options will vary by region. For more information, please visit our <a href=\"https:\/\/eu.super73.com\/pages\/financing-payments\" title=\"https:\/\/eu.super73.com\/pages\/financing-payments\">Financing and Payment Options<\/a> page.<\/p>",
